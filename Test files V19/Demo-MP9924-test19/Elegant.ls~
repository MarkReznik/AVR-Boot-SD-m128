00268b 81e8      	LD   R30,Y
00268c 93e0 0782 	STS  _TimeSpike,R30
         ;    5533 
         ;    5534        // Port F
         ;    5535        PORTF=0x00;
00268e 940e 47c9 	CALL SUBOPT_0x0
         ;    5536        DDRF=0x00;     // output     
         ;    5537 }  
002690 9623      	ADIW R28,3
002691 9508      	RET
         ;    5538 void StopMessage(void)
         ;    5539 {                  
