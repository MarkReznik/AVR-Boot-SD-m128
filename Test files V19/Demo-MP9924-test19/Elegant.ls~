0027f2 30e0      	CPI  R30,0
0027f3 f411      	BRNE _0x5D7
         ;    5856                                       Button = STAY;
0027f4 940e 4d4f 	CALL SUBOPT_0xC4
         ;    5857                                       ButtonMenu();                                           
         ;    5858                                }      
         ;    5859                                Button = Key;  
          _0x5D7:
0027f6 80f8      	LDD  R15,Y+0
         ;    5860                                ButtonMenu();                    
0027f7 940e 1677 	CALL _ButtonMenu
         ;    5861                         }                        
         ;    5862                         if(StatusArm){
