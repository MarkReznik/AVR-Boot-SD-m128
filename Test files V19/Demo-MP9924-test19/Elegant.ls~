         ;AT THE START OF FLASH
0005bf e0f1      	LDI  R31,1
0005c0 bff5      	OUT  MCUCR,R31
0005c1 bfe5      	OUT  MCUCR,R30
0005c2 93e0 006c 	STS  XMCRB,R30
0005c4 bfeb      	OUT  RAMPZ,R30
         
