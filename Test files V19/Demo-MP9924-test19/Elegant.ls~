         ;    9735 void Write_Date(void)
         ;    9736 {        
          _Write_Date:
         ;    9737      Pointer = TimeDate;
004443 940e 4bf1 	CALL SUBOPT_0x99
         ;    9738      MaxNum = 4;
         ;    9739      Data[0] = Day;
004445 91e0 07e2 	LDS  R30,_Day
