         ;   10097 {      
          _WriteBypass:
         ;   10098     char i; 
         ;   10099     Pointer = Status_SAC;
0044e9 940e 4f89 	CALL SUBOPT_0x123
         ;	i -> R16
         ;   10100     MaxNum = 16;
         ;   10101     ReadData(1);  
0044eb 940e 4ba4 	CALL SUBOPT_0x8D
         ;   10102     for(i = 0;i < 8;i++){
0044ed e000      	LDI  R16,LOW(0)
          _0x9BC:
0044ee 3008      	CPI  R16,8
0044ef f530      	BRSH _0x9BD
         ;   10103          if(Zone[i] & 0x40)
0044f0 940e 4961 	CALL SUBOPT_0x3C
0044f2 81e0      	LD   R30,Z
0044f3 74e0      	ANDI R30,LOW(0x40)
0044f4 f069      	BREQ _0x9BE
         ;   10104              Data[3]|= (0x01<<i);  
