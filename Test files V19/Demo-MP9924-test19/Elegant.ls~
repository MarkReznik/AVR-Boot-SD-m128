         ;    1110 void main(void)
         ;    1111 {                 
         
          	.CSEG
          _main:
         ;    1112    WDTCR = 0x00;
0005f0 27ee      	CLR  R30
0005f1 bde1      	OUT  0x21,R30
         ;    1113 // Declare your local variables here
         ;    1114 
         ;    1115 // Input/Output Ports initialization   
         ;    1116 // Output - 1
         ;    1117 // Input  - 0     
         ;    1118 // port 0 - NO
         ;    1119 // port 1 - Yes 
         ;    1120 // Port A
         ;    1121 PORTA=0x40;  //PORTA|= 0x40; 
0005f2 e4e0      	LDI  R30,LOW(64)
0005f3 bbeb      	OUT  0x1B,R30
         ;    1122 DDRA=0x40;
0005f4 bbea      	OUT  0x1A,R30
         ;    1123 
         ;    1124 // Port B
         ;    1125 PORTB=0x00;
0005f5 27ee      	CLR  R30
0005f6 bbe8      	OUT  0x18,R30
         ;    1126 DDRB=0x70;
0005f7 e7e0      	LDI  R30,LOW(112)
0005f8 bbe7      	OUT  0x17,R30
         ;    1127 
         ;    1128 // Port C
         ;    1129 PORTC=0x18;
0005f9 e1e8      	LDI  R30,LOW(24)
0005fa bbe5      	OUT  0x15,R30
         ;    1130 DDRC=0xE7;
0005fb eee7      	LDI  R30,LOW(231)
0005fc bbe4      	OUT  0x14,R30
         ;    1131 
         ;    1132 // Port D
         ;    1133 PORTD=0x50;
0005fd e5e0      	LDI  R30,LOW(80)
0005fe bbe2      	OUT  0x12,R30
         ;    1134 //DDRD=0xC8; 
         ;    1135 DDRD=0xDC; 
0005ff edec      	LDI  R30,LOW(220)
000600 bbe1      	OUT  0x11,R30
         ;    1136 // Port E
         ;    1137 PORTE=0x07;
000601 e0e7      	LDI  R30,LOW(7)
000602 b9e3      	OUT  0x3,R30
         ;    1138 DDRE=0x0E;         
000603 e0ee      	LDI  R30,LOW(14)
000604 b9e2      	OUT  0x2,R30
         ;    1139 
         ;    1140 // Port F
         ;    1141 PORTF=0x00;
000605 940e 47c9 	CALL SUBOPT_0x0
         ;    1142 DDRF=0x00;
         ;    1143 
         ;    1144 // Port G
         ;    1145 PORTG=0x04;
000607 e0e4      	LDI  R30,LOW(4)
000608 93e0 0065 	STS  0x65,R30
         ;    1146 DDRG=0x04;
00060a 93e0 0064 	STS  0x64,R30
         ;    1147 
         ;    1148 // Timer/Counter 0 initialization
         ;    1149 // Clock source: System Clock
         ;    1150 // Clock value: 15.625 kHz
         ;    1151 // Mode: Normal top=FFh
         ;    1152 // OC0 output: Disconnected
         ;    1153 TCCR0=0x06;
00060c e0e6      	LDI  R30,LOW(6)
00060d bfe3      	OUT  0x33,R30
         ;    1154 ASSR=0x00;
00060e 27ee      	CLR  R30
00060f bfe0      	OUT  0x30,R30
         ;    1155 TCNT0=0x4B;
000610 e4eb      	LDI  R30,LOW(75)
000611 bfe2      	OUT  0x32,R30
         ;    1156 OCR0=0x00;
000612 27ee      	CLR  R30
000613 bfe1      	OUT  0x31,R30
         ;    1157 
         ;    1158 // Timer/Counter 1 initialization
         ;    1159 // Clock source: System Clock
         ;    1160 // Clock value: Timer 1 Stopped
         ;    1161 // Mode: Normal top=FFFFh
         ;    1162 // OC1A output: Discon.
         ;    1163 // OC1B output: Discon.
         ;    1164 // OC1C output: Discon.
         ;    1165 // Noise Canceler: Off
         ;    1166 // Input Capture on Falling Edge
         ;    1167 TCCR1A=0x00;
000614 bdef      	OUT  0x2F,R30
         ;    1168 TCCR1B=0x00;
000615 bdee      	OUT  0x2E,R30
         ;    1169 TCNT1H=0x00;
000616 bded      	OUT  0x2D,R30
         ;    1170 TCNT1L=0x00;
000617 bdec      	OUT  0x2C,R30
         ;    1171 OCR1AH=0x00;
000618 bdeb      	OUT  0x2B,R30
         ;    1172 OCR1AL=0x00;
000619 bdea      	OUT  0x2A,R30
         ;    1173 OCR1BH=0x00;
00061a bde9      	OUT  0x29,R30
         ;    1174 OCR1BL=0x00;
00061b bde8      	OUT  0x28,R30
         ;    1175 OCR1CH=0x00;
00061c 93e0 0079 	STS  0x79,R30
         ;    1176 OCR1CL=0x00;
00061e 93e0 0078 	STS  0x78,R30
         ;    1177 
         ;    1178 // Timer/Counter 2 initialization
         ;    1179 // Clock source: System Clock
         ;    1180 // Clock value: 15.625 kHz
         ;    1181 // Mode: Normal top=FFh
         ;    1182 // OC2 output: Disconnected  
