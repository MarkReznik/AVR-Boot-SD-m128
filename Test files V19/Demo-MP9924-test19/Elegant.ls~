         ;    3977 void SIREN_OFF(void)
         ;    3978 {                 
          _SIREN_OFF:
         ;    3979      DataMoked[pStopEvent][0] = SIR_OFF;
001f82 940e 4933 	CALL SUBOPT_0x36
001f84 0fae      	ADD  R26,R30
001f85 1fbf      	ADC  R27,R31
