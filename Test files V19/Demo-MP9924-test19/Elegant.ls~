         ;    6201              if(StatusArm){      
002a3c 91e0 0658 	LDS  R30,_StatusArm
002a3e 30e0      	CPI  R30,0
002a3f f411      	BRNE PC+3
002a40 940c 2a8b 	JMP _0x64F
         ;    6202                      if(StatusArm == 1){    
002a42 91a0 0658 	LDS  R26,_StatusArm
002a44 30a1      	CPI  R26,LOW(0x1)
002a45 f4d9      	BRNE _0x650
         ;    6203                            
         ;    6204                            if((StatusVolt & 0x07)||(bZoneType >= 0x40 )||((bZoneType > 0x08)&&!(StatusY_N & 0x01))){
002a46 91e0 0659 	LDS  R30,_StatusVolt
002a48 70e7      	ANDI R30,LOW(0x7)
002a49 f461      	BRNE _0x652
002a4a 91a0 07cf 	LDS  R26,_bZoneType
002a4c 34a0      	CPI  R26,LOW(0x40)
002a4d f440      	BRSH _0x652
002a4e 940e 4929 	CALL SUBOPT_0x34
002a50 f420      	BRSH _0x653
002a51 91e0 063f 	LDS  R30,_StatusY_N
002a53 70e1      	ANDI R30,LOW(0x1)
