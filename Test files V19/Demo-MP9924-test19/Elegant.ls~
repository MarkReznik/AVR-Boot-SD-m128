         ;    5863                                 if(Key == '*'){        
0027fd 81a8      	LD   R26,Y
0027fe 32aa      	CPI  R26,LOW(0x2A)
0027ff f531      	BRNE _0x5D9
         ;    5864                                        if((UserCode[0]==0x33)&&(CountCode == 1)){
002800 91a0 065f 	LDS  R26,_UserCode
002802 33a3      	CPI  R26,LOW(0x33)
