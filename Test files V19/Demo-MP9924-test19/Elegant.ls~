         ;    5724 char ConectToTel(char CountTelephon)
         ;    5725 {
          _ConectToTel:
         ;    5726     char i;         
         ;    5727     strcpyf(tx_buffer1,"ATDT");
002777 930a      	ST   -Y,R16
         ;	CountTelephon -> Y+1
         ;	i -> R16
002778 ede5      	LDI  R30,LOW(_tx_buffer1)
002779 e0f6      	LDI  R31,HIGH(_tx_buffer1)
