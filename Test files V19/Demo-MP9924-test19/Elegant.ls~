         ;    5547 void GetMessage(int Address)
         ;    5548 {            
          _GetMessage:
         ;    5549        // Port F
         ;    5550        PORTF=0x00;
00269b 940e 4ce1 	CALL SUBOPT_0xB4
         ;    5551        DDRF=0xFF;     // output           
         ;    5552 
         ;    5553        PORTC|= 0x40;        // disenable                      
00269d 9aae      	SBI  0x15,6
         ;    5554        PORTF = Address;        // Address   
00269e 81e8      	LD   R30,Y
00269f 81f9      	LDD  R31,Y+1
0026a0 940e 4ce8 	CALL SUBOPT_0xB5
         ;    5555        PORTC&=~0x03;  
         ;    5556        PORTC|= (0x03 & (Address>>8));        // Address    
0026a2 93ef      	PUSH R30
0026a3 81e8      	LD   R30,Y
0026a4 81f9      	LDD  R31,Y+1
0026a5 940e 504d 	CALL __ASRw8
0026a7 70e3      	ANDI R30,LOW(0x3)
0026a8 70f0      	ANDI R31,HIGH(0x3)
0026a9 91af      	POP  R26
0026aa 940e 4cf9 	CALL SUBOPT_0xB6
         ;    5557        
         ;    5558        PauseSpike = 5;              
         ;    5559        while(PauseSpike);   
          _0x56B:
0026ac 91e0 0784 	LDS  R30,_PauseSpike
0026ae 30e0      	CPI  R30,0
0026af f7e1      	BRNE _0x56B
         ;    5560               
         ;    5561        PORTC&=~0x80;        // PD  
0026b0 940e 4d01 	CALL SUBOPT_0xB7
0026b2 940e 4d08 	CALL SUBOPT_0xB8
         ;    5562        #asm("NOP");                 
0026b4 0000      	NOP
         ;    5563        PORTC|= 0x20;        // P/R   
0026b5 9aad      	SBI  0x15,5
         ;    5564        #asm("NOP"); 
0026b6 0000      	NOP
         ;    5565        PORTC&=~0x40;        // enable   
0026b7 940e 4d01 	CALL SUBOPT_0xB7
0026b9 940e 4d10 	CALL SUBOPT_0xB9
         ;    5566            
         ;    5567        StatusSpike = 2;
0026bb e0e2      	LDI  R30,LOW(2)
0026bc 93e0 0781 	STS  _StatusSpike,R30
         ;    5568        
         ;    5569        // Port F
         ;    5570        PORTF=0x00;
