         ;    4556 sfrb SFIOR=0x20;
         ;    4557 sfrb WDTCR=0x21;
         ;    4558 sfrb OCDR=0x22;
         ;    4559 sfrb OCR2=0x23;
         ;    4560 sfrb TCNT2=0x24;
         ;    4561 sfrb TCCR2=0x25;
         ;    4562 sfrb ICR1L=0x26;
         ;    4563 sfrb ICR1H=0x27;
         ;    4564 sfrw ICR1=0x26;   // 16 bit access
         ;    4565 sfrb OCR1BL=0x28;
         ;    4566 sfrb OCR1BH=0x29;
         ;    4567 sfrw OCR1B=0x28;  // 16 bit access
         ;    4568 sfrb OCR1AL=0x2a;
         ;    4569 sfrb OCR1AH=0x2b;
         ;    4570 sfrw OCR1A=0x2a;  // 16 bit access
         ;    4571 sfrb TCNT1L=0x2c;
         ;    4572 sfrb TCNT1H=0x2d;
         ;    4573 sfrw TCNT1=0x2c;  // 16 bit access
         ;    4574 sfrb TCCR1B=0x2e;
         ;    4575 sfrb TCCR1A=0x2f;
         ;    4576 sfrb ASSR=0x30;
         ;    4577 sfrb OCR0=0x31;
         ;    4578 sfrb TCNT0=0x32;
         ;    4579 sfrb TCCR0=0x33;
         ;    4580 sfrb MCUCSR=0x34;
         ;    4581 sfrb MCUCR=0x35;
         ;    4582 sfrb TIFR=0x36;
         ;    4583 sfrb TIMSK=0x37;
         ;    4584 sfrb EIFR=0x38;
         ;    4585 sfrb EIMSK=0x39;
