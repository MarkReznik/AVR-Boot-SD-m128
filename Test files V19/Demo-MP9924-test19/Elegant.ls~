         ;   10038 void WriteCountMadCode(void)   // write counter mad code to eeprom
         ;   10039 {
          _WriteCountMadCode:
         ;   10040     Pointer = Status_SAC;
00448b e1e8      	LDI  R30,LOW(536)
00448c e0f2      	LDI  R31,HIGH(536)
00448d 940e 4966 	CALL SUBOPT_0x3D
         ;   10041     MaxNum = 16;
         ;   10042     ReadData(1);
00448f 940e 4ba4 	CALL SUBOPT_0x8D
         ;   10043     Data[5] = CountMadCode;
004491   +  	__POINTW2MN _Data,5
004491 efa2      	LDI  R26,LOW(_data+0x5)
004492 e0b5      	LDI  R27,HIGH(_data+0x5)
          	.ENDM
004493 91e0 0671 	LDS  R30,_CountMadCode
004495 93ec      	ST   X,R30
         ;   10044     WriteData(1);    
004496 940e 4bed 	CALL SUBOPT_0x98
         ;   10045 }
