          _0x83A:
         ;    8013    size = strlen(str);  
          _0x7FD:
003ba6 81e9      	LDD  R30,Y+1
003ba7 81fa      	LDD  R31,Y+1+1
003ba8 93fa      	ST   -Y,R31
003ba9 93ea      	ST   -Y,R30
003baa 940e 4fea 	CALL _strlen
003bac 83e8      	ST   Y,R30
         ;    8014    for(;size<16;size++)
          _0x83C:
003bad 81a8      	LD   R26,Y
003bae 31a0      	CPI  R26,LOW(0x10)
003baf f448      	BRSH _0x83D
         ;    8015       str[size] = ' '; 
003bb0 940e 4b5c 	CALL SUBOPT_0x81
003bb2 0fae      	ADD  R26,R30
003bb3 1fbf      	ADC  R27,R31
003bb4 e2e0      	LDI  R30,LOW(32)
003bb5 93ec      	ST   X,R30
003bb6 940e 4b58 	CALL SUBOPT_0x80
003bb8 cff4      	RJMP _0x83C
          _0x83D:
         ;    8016    
         ;    8017 }
003bb9 9625      	ADIW R28,5
003bba 9508      	RET
         ;    8018 void FuncNumber(unsigned char Num,char* str)
         ;    8019 {
          _FuncNumber:
         ;    8020   unsigned char Azer[3];
         ;    8021       
         ;    8022      Azer[0] = Num/10 + 0x30;
003bbb 9723      	SBIW R28,3
         ;	Num -> Y+5
         ;	*str -> Y+3
         ;	Azer -> Y+0
003bbc 81ad      	LDD  R26,Y+5
003bbd 940e 4ec9 	CALL SUBOPT_0x104
003bbf 83e8      	ST   Y,R30
         ;    8023      Azer[1] = Num%10 + 0x30;
003bc0 81ad      	LDD  R26,Y+5
003bc1 940e 4ece 	CALL SUBOPT_0x105
003bc3 83e9      	STD  Y+1,R30
         ;    8024      Azer[2] = 0;
003bc4 27ee      	CLR  R30
003bc5 83ea      	STD  Y+2,R30
         ;    8025      strcat(str,Azer);
003bc6 81eb      	LDD  R30,Y+3
003bc7 81fc      	LDD  R31,Y+3+1
003bc8 940e 4ed3 	CALL SUBOPT_0x106
         ;    8026     
         ;    8027 }                     
003bca 9626      	ADIW R28,6
003bcb 9508      	RET
         ;    8028 void FuncNumberH(unsigned char Num,char* str)
         ;    8029 {
          _FuncNumberH:
         ;    8030     unsigned char Azer[4];
         ;    8031       
         ;    8032      Azer[0] = Num/100 + 0x30;  
003bcc 9724      	SBIW R28,4
         ;	Num -> Y+6
         ;	*str -> Y+4
         ;	Azer -> Y+0
003bcd 81ae      	LDD  R26,Y+6
003bce e6e4      	LDI  R30,LOW(100)
003bcf 940e 505a 	CALL __DIVB21U
003bd1 5de0      	SUBI R30,-LOW(48)
003bd2 83e8      	ST   Y,R30
         ;    8033      Num%= 100;
003bd3 81ae      	LDD  R26,Y+6
003bd4 e6e4      	LDI  R30,LOW(100)
003bd5 940e 507b 	CALL __MODB21U
003bd7 83ee      	STD  Y+6,R30
         ;    8034      Azer[1] = Num/10 + 0x30;
003bd8 81ae      	LDD  R26,Y+6
003bd9 940e 4ec9 	CALL SUBOPT_0x104
003bdb 83e9      	STD  Y+1,R30
         ;    8035      Azer[2] = Num%10 + 0x30;
003bdc 81ae      	LDD  R26,Y+6
003bdd 940e 4ece 	CALL SUBOPT_0x105
003bdf 83ea      	STD  Y+2,R30
         ;    8036      Azer[3] = 0;
003be0 27ee      	CLR  R30
003be1 83eb      	STD  Y+3,R30
         ;    8037      strcat(str,Azer);
003be2 81ec      	LDD  R30,Y+4
003be3 81fd      	LDD  R31,Y+4+1
003be4 940e 4ed3 	CALL SUBOPT_0x106
         ;    8038 }
003be6 9627      	ADIW R28,7
003be7 9508      	RET
