          _indStartMoked:
00072e      	.BYTE 0x1
         ;    5316 //unsigned char TelData;
         ;    5317 unsigned char DataMoked[20][2];   
          _DataMoked:
00072f      	.BYTE 0x28
         ;    5318 unsigned char pStrSpike[20];              // string of word to speak   
          _pStrSpike:
000757      	.BYTE 0x14
         ;    5319 unsigned int StrInt[6];              // string of word to speak
          _StrInt:
00076b      	.BYTE 0xC
         ;    5320 unsigned char dStr, nStr, MaxStr;        
          _dStr:
