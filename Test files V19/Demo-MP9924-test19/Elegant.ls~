         ;    2912 void ButtonMenu(void)      // mangment of Button from keybord
         ;    2913 {   
          _ButtonMenu:
         ;    2914   //unsigned int Pointer;
         ;    2915     if(Status == 0)
001677 91e0 05d9 	LDS  R30,_Status
001679 30e0      	CPI  R30,0
00167a f541      	BRNE _0x305
         ;    2916     {                                              //Start
         ;    2917         if(Button == MENU)       
00167b e1e6      	LDI  R30,LOW(22)
00167c 15ef      	CP   R30,R15
00167d f511      	BRNE _0x306
         ;    2918         {     
         ;    2919             if((!CycleDial)&&(!StatusArm)&&(!ValueSiren)&&(!ValueExit)) 
00167e 91e0 0720 	LDS  R30,_CycleDial
001680 30e0      	CPI  R30,0
001681 f461      	BRNE _0x308
001682 91e0 0658 	LDS  R30,_StatusArm
001684 30e0      	CPI  R30,0
001685 f441      	BRNE _0x308
001686 91e0 0647 	LDS  R30,_ValueSiren
001688 30e0      	CPI  R30,0
001689 f421      	BRNE _0x308
00168a 91e0 0653 	LDS  R30,_ValueExit
00168c 30e0      	CPI  R30,0
