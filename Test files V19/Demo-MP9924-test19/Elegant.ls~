         ;    7510 void String(unsigned int NumOfStr,char* str)
         ;    7511 {
          _String:
         ;    7512  // unsigned char Azer[2];
         ;    7513   unsigned char size;
         ;    7514   
         ;    7515   if(!Status)
003546 9721      	SBIW R28,1
         ;	NumOfStr -> Y+3
         ;	*str -> Y+1
         ;	size -> Y+0
003547 91e0 05d9 	LDS  R30,_Status
003549 30e0      	CPI  R30,0
00354a f011      	BREQ PC+3
00354b 940c 36c7 	JMP _0x7AF
         ;    7516   {
         ;    7517      if(StatusArm)
00354d 91e0 0658 	LDS  R30,_StatusArm
00354f 30e0      	CPI  R30,0
003550 f411      	BRNE PC+3
003551 940c 3620 	JMP _0x7B0
         ;    7518      {
         ;    7519        
         ;    7520         if((int)NumOfStr == 0);
003553 81eb      	LDD  R30,Y+3
003554 81fc      	LDD  R31,Y+3+1
003555 9730      	SBIW R30,0
003556 f401      	BRNE _0x7B1
         ;    7521            if(Panic)
          _0x7B1:
003557 fe31      	SBRS R3,1
003558 c007      	RJMP _0x7B2
         ;    7522               strcpyf(str,"     Panic");
003559 81e9      	LDD  R30,Y+1
00355a 81fa      	LDD  R31,Y+1+1
00355b 93fa      	ST   -Y,R31
