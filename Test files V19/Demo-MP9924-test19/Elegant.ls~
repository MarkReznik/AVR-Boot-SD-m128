         ;    5728     Pointer = Telephone;                   
002782 e3e4      	LDI  R30,LOW(308)
002783 e0f1      	LDI  R31,HIGH(308)
002784 940e 4966 	CALL SUBOPT_0x3D
         ;    5729     MaxNum = 16;      
         ;    5730     ReadData(CountTelephon);
002786 940e 496e 	CALL SUBOPT_0x3E
         ;    5731     for(i=0;(i<16)&&(Data[i]>0x20);i++)
002788 e000      	LDI  R16,LOW(0)
          _0x5A7:
002789 3100      	CPI  R16,16
00278a f428      	BRSH _0x5A9
00278b 940e 4af2 	CALL SUBOPT_0x71
00278d 940e 4b7e 	CALL SUBOPT_0x87
00278f f008      	BRLO _0x5AA
          _0x5A9:
