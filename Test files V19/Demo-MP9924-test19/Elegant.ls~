003492   +  	__GETWRN 16,17,428
003492 ea0c      	LDI  R0x10,LOW(0x1ac)
003493 e011      	LDI  R0x11,HIGH(0x1ac)
          	.ENDM
         ;    6904       if(Word == '_')  Address = 0x0200;    // MESSAGE 1  
          _0x775:
003494 81aa      	LDD  R26,Y+2
003495 35af      	CPI  R26,LOW(0x5F)
003496 f411      	BRNE _0x776
003497   +  	__GETWRN 16,17,512
003497 e000      	LDI  R0x10,LOW(0x200)
003498 e012      	LDI  R0x11,HIGH(0x200)
          	.ENDM
         ;    6905       if(Word == '')  Address = 0x0240;    // MESSAGE 2
          _0x776:
003499 81aa      	LDD  R26,Y+2
00349a 3fa9      	CPI  R26,LOW(0xF9)
00349b f411      	BRNE _0x777
00349c   +  	__GETWRN 16,17,576
00349c e400      	LDI  R0x10,LOW(0x240)
00349d e012      	LDI  R0x11,HIGH(0x240)
          	.ENDM
         ;    6906             
         ;    6907             
         ;    6908       GetMessage(Address);       
          _0x777:
00349e 931a      	ST   -Y,R17
00349f 930a      	ST   -Y,R16
0034a0 940e 269b 	CALL _GetMessage
         ;    6909 
         ;    6910 }
0034a2 c010      	RJMP _0xAB1
         ;    6911 char TestStatusZone(char Bit)
         ;    6912 {
          _TestStatusZone:
         ;    6913        // please enter code
         ;    6914        char i, Find = 0;
         ;    6915        for(i=0;i<16;i++)
0034a3 931a      	ST   -Y,R17
0034a4 930a      	ST   -Y,R16
         ;	Bit -> Y+2
         ;	i -> R16
         ;	Find -> R17
0034a5 e010      	LDI  R17,0
0034a6 e000      	LDI  R16,LOW(0)
          _0x77A:
0034a7 3100      	CPI  R16,16
0034a8 f448      	BRSH _0x77B
         ;    6916           if(Zone[i] & Bit)
0034a9 940e 4961 	CALL SUBOPT_0x3C
0034ab 81e0      	LD   R30,Z
0034ac 81aa      	LDD  R26,Y+2
0034ad 23ea      	AND  R30,R26
0034ae f009      	BREQ _0x77C
         ;    6917                Find = 1;
0034af e011      	LDI  R17,LOW(1)
         ;    6918        return(Find);  
          _0x77C:
0034b0 5f0f      	SUBI R16,-1
0034b1 cff5      	RJMP _0x77A
          _0x77B:
0034b2 2fe1      	MOV  R30,R17
          _0xAB1:
0034b3 8119      	LDD  R17,Y+1
0034b4 8108      	LDD  R16,Y+0
0034b5 9623      	ADIW R28,3
0034b6 9508      	RET
         ;    6919 }
