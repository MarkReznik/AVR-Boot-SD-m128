002cdc f579      	BRNE _0x68F
         ;    6463                            if(StutLine == 0){                  // System arm Away/Stay mode 1/2   
002cdd 81ea      	LDD  R30,Y+2
002cde 81fb      	LDD  R31,Y+2+1
