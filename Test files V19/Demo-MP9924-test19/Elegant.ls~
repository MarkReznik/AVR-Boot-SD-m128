         ;    4008 char ReturnTime(char Place)    // return time from eeprom(Status time and Cycle)
         ;    4009 {
          _ReturnTime:
         ;    4010     Pointer = SysTimeCycle;
001fc9 e2e8      	LDI  R30,LOW(552)
001fca e0f2      	LDI  R31,HIGH(552)
001fcb 940e 4966 	CALL SUBOPT_0x3D
         ;    4011     MaxNum = 16;
         ;    4012     ReadData(1);
001fcd 940e 4ba4 	CALL SUBOPT_0x8D
         ;    4013     if((Place < 10)||(Place == 14))                         
001fcf 81a8      	LD   R26,Y
001fd0 30aa      	CPI  R26,LOW(0xA)
001fd1 f010      	BRLO _0x49E
001fd2 30ae      	CPI  R26,LOW(0xE)
001fd3 f469      	BRNE _0x49D
          _0x49E:
         ;    4014       return(((Data[Place]&0x0F)*10) + (Data[Place+1]&0x0F));
001fd4 940e 4b52 	CALL SUBOPT_0x7F
