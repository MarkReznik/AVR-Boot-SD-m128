00408e 93e0 07d7 	STS  _CountEventH,R30
         ;    9192     CountEventM = 0;
004090 93e0 07d8 	STS  _CountEventM,R30
         ;    9193     
         ;    9194     WriteEvent();
004092 d134      	RCALL _WriteEvent
         ;    9195 }  
004093 8119      	LDD  R17,Y+1
004094 8108      	LDD  R16,Y+0
004095 9624      	ADIW R28,4
004096 9508      	RET
