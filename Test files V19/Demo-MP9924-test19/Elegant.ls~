0044e4 5f0f      	SUBI R16,-1
0044e5 cfd6      	RJMP _0x9B0
          _0x9B1:
         ;   10081     WriteData(1);
0044e6 940e 4bed 	CALL SUBOPT_0x98
         ;   10082 }     
0044e8 c06f      	RJMP _0xAAC
         ;   10083 void ReadActivate(void)            // read bit of Activate
         ;   10084 {         
