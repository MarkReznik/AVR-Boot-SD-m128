         ;    6457 void StrSpike(char* StrSpike,int StutLine)
         ;    6458 {             
          _StrSpike:
         ;    6459         char i,j;
         ;    6460         if(!Status){
002ccc 9722      	SBIW R28,2
         ;	*StrSpike -> Y+4
         ;	StutLine -> Y+2
         ;	i -> Y+1
         ;	j -> Y+0
