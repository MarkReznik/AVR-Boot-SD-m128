         ;    6196 
         ;    6197 void tSpikeSystem(int* StrInt){   
          _tSpikeSystem:
         ;    6198       char ind;  
         ;    6199       NewSpike = 1;
002a32 9721      	SBIW R28,1
         ;	*StrInt -> Y+1
         ;	ind -> Y+0
002a33 e0e1      	LDI  R30,LOW(1)
002a34 93e0 077d 	STS  _NewSpike,R30
         ;    6200       if(!Status){
