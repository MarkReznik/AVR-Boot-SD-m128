00462a 5de0      	SUBI R30,-LOW(48)
00462b   +  	__PUTB1MN _Data,15
00462b 93e0 05fc 	STS  _data+0xf,R30
          	.ENDM
         ;   10452      }
         ;   10453      if((Arm == 3)||(Arm == 4)){
          _0x9E4:
