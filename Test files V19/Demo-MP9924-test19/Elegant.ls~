         ;    5835 
         ;    5836 
         ;    5837 
         ;    5838 char tTestCode(void)
         ;    5839 {            
          _tTestCode:
         ;    5840     //  enter code         
         ;    5841     
         ;    5842     if((tUserCode[0] == 1)&&( tUserCode[1] == 1)&&(tUserCode[2] == 0)) 
0027cf 91a0 0723 	LDS  R26,_tUserCode
0027d1 30a1      	CPI  R26,LOW(0x1)
0027d2 f441      	BRNE _0x5CC
0027d3   +  	__GETB1MN _tUserCode,1
0027d3 91e0 0724 	LDS  R30,_tusercode+0x1
          	.ENDM
