          	.DSEG
          _StrTelTone:
000819      	.BYTE 0x11
         ;   11017 unsigned char  GoTel, indTone, MaxTone;
          _GoTel:
00082a      	.BYTE 0x1
          _indTone:
00082b      	.BYTE 0x1
          _MaxTone:
00082c      	.BYTE 0x1
         ;   11018 unsigned char  SecondTone, TimePauseTone,TimePausePul; 
          _SecondTone:
00082d      	.BYTE 0x1
          _TimePauseTone:
00082e      	.BYTE 0x1
          _TimePausePul:
00082f      	.BYTE 0x1
         ;   11019 unsigned char  ChangePul;
          _ChangePul:
000830      	.BYTE 0x1
         ;   11020  void SendTelephone(char Number)
         ;   11021 {                
         
          	.CSEG
         ;   11022       char i;   
         ;   11023       Pointer = Telephone;                   
         ;	Number -> Y+1
         ;	i -> R16
         ;   11024       MaxNum = 16;      
         ;   11025       ReadData(Number);
