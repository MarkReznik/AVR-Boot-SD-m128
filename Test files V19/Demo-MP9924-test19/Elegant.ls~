0007f0      	.BYTE 0x1
         ;   10426 char StatusTimer[4][MaxTimer];
          _StatusTimer:
0007f1      	.BYTE 0x28
         ;   10427 void DisplayTimer(char num)
         ;   10428 {    
         
          	.CSEG
          _DisplayTimer:
         ;   10429      char i;
         ;   10430      char Day, Hour, Min, Arm; 
         ;   10431      num--;
0045db 940e 4f99 	CALL SUBOPT_0x127
         ;	num -> Y+5
         ;	i -> R16
         ;	Day -> R17
         ;	Hour -> R18
         ;	Min -> R19
         ;	Arm -> R20
         ;   10432      Day  = StatusTimer[0][num];
0045dd 81ed      	LDD  R30,Y+5
0045de 940e 4f9f 	CALL SUBOPT_0x128
         ;   10433      Hour = StatusTimer[1][num];
0045e0   +  	__POINTW2MN _StatusTimer,10
0045e0 efab      	LDI  R26,LOW(_statustimer+0xa)
0045e1 e0b7      	LDI  R27,HIGH(_statustimer+0xa)
          	.ENDM
0045e2 940e 4fa4 	CALL SUBOPT_0x129
0045e4 912c      	LD   R18,X
         ;   10434      Min  = StatusTimer[2][num];
0045e5   +  	__POINTW2MN _StatusTimer,20
0045e5 e0a5      	LDI  R26,LOW(_statustimer+0x14)
0045e6 e0b8      	LDI  R27,HIGH(_statustimer+0x14)
          	.ENDM
0045e7 940e 4fa4 	CALL SUBOPT_0x129
0045e9 913c      	LD   R19,X
         ;   10435      Arm  = StatusTimer[3][num];
0045ea   +  	__POINTW2MN _StatusTimer,30
0045ea e0af      	LDI  R26,LOW(_statustimer+0x1e)
0045eb e0b8      	LDI  R27,HIGH(_statustimer+0x1e)
          	.ENDM
0045ec 940e 4fa4 	CALL SUBOPT_0x129
0045ee 914c      	LD   R20,X
         ;   10436      for(i = 0;i < 7; i++){
0045ef e000      	LDI  R16,LOW(0)
          _0x9E0:
0045f0 3007      	CPI  R16,7
0045f1 f498      	BRSH _0x9E1
         ;   10437           if(Day & (0x01<<i))
0045f2 940e 4e95 	CALL SUBOPT_0xFC
0045f4 23e1      	AND  R30,R17
0045f5 f049      	BREQ _0x9E2
         ;   10438               Data[i] = 0x31 + i;
0045f6 940e 4c84 	CALL SUBOPT_0xA7
0045f8 93ff      	PUSH R31
0045f9 93ef      	PUSH R30
0045fa 2fe0      	MOV  R30,R16
0045fb 5cef      	SUBI R30,-LOW(49)
0045fc 91af      	POP  R26
0045fd 91bf      	POP  R27
0045fe c003      	RJMP _0xB04
         ;   10439           else
          _0x9E2:
         ;   10440               Data[i] = '*';    
0045ff 940e 4972 	CALL SUBOPT_0x3F
004601 e2ea      	LDI  R30,LOW(42)
          _0xB04:
004602 93ec      	ST   X,R30
         ;   10441      }                     
004603 5f0f      	SUBI R16,-1
004604 cfeb      	RJMP _0x9E0
          _0x9E1:
         ;   10442      Data[7] = ' ';
004605 e2e0      	LDI  R30,LOW(32)
004606   +  	__PUTB1MN _Data,7
004606 93e0 05f4 	STS  _data+0x7,R30
          	.ENDM
         ;   10443      Data[8]  = 0x30 + (Hour/10);
004608 2fa2      	MOV  R26,R18
004609 940e 4ec9 	CALL SUBOPT_0x104
00460b   +  	__PUTB1MN _Data,8
00460b 93e0 05f5 	STS  _data+0x8,R30
          	.ENDM
         ;   10444      Data[9]  = 0x30 + (Hour%10);   
00460d 2fa2      	MOV  R26,R18
00460e 940e 4ece 	CALL SUBOPT_0x105
004610   +  	__PUTB1MN _Data,9
004610 93e0 05f6 	STS  _data+0x9,R30
          	.ENDM
         ;   10445      Data[10] = ':';
004612 e3ea      	LDI  R30,LOW(58)
004613   +  	__PUTB1MN _Data,10
004613 93e0 05f7 	STS  _data+0xa,R30
          	.ENDM
         ;   10446      Data[11] = 0x30 + (Min/10);
004615 2fa3      	MOV  R26,R19
004616 940e 4ec9 	CALL SUBOPT_0x104
004618   +  	__PUTB1MN _Data,11
004618 93e0 05f8 	STS  _data+0xb,R30
          	.ENDM
         ;   10447      Data[12] = 0x30 + (Min%10);
00461a 2fa3      	MOV  R26,R19
00461b 940e 4ece 	CALL SUBOPT_0x105
00461d   +  	__PUTB1MN _Data,12
00461d 93e0 05f9 	STS  _data+0xc,R30
          	.ENDM
         ;   10448      Data[13] = ' ';  
00461f e2e0      	LDI  R30,LOW(32)
004620   +  	__PUTB1MN _Data,13
004620 93e0 05fa 	STS  _data+0xd,R30
          	.ENDM
         ;   10449      if((Arm == 1)||(Arm == 2)){
004622 3041      	CPI  R20,1
004623 f011      	BREQ _0x9E5
004624 3042      	CPI  R20,2
004625 f439      	BRNE _0x9E4
          _0x9E5:
         ;   10450           Data[14] = 'S';
004626 e5e3      	LDI  R30,LOW(83)
004627   +  	__PUTB1MN _Data,14
004627 93e0 05fb 	STS  _data+0xe,R30
