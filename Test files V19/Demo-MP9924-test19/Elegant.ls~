         ;    8076 sfrb DDRA=0x1a;
         ;    8077 sfrb PORTA=0x1b;
         ;    8078 sfrb EECR=0x1c;
         ;    8079 sfrb EEDR=0x1d;
         ;    8080 sfrb EEARL=0x1e;
         ;    8081 sfrb EEARH=0x1f;
         ;    8082 sfrw EEAR=0x1e;   // 16 bit access
         ;    8083 sfrb SFIOR=0x20;
         ;    8084 sfrb WDTCR=0x21;
         ;    8085 sfrb OCDR=0x22;
         ;    8086 sfrb OCR2=0x23;
         ;    8087 sfrb TCNT2=0x24;
         ;    8088 sfrb TCCR2=0x25;
         ;    8089 sfrb ICR1L=0x26;
         ;    8090 sfrb ICR1H=0x27;
         ;    8091 sfrw ICR1=0x26;   // 16 bit access
         ;    8092 sfrb OCR1BL=0x28;
         ;    8093 sfrb OCR1BH=0x29;
         ;    8094 sfrw OCR1B=0x28;  // 16 bit access
         ;    8095 sfrb OCR1AL=0x2a;
         ;    8096 sfrb OCR1AH=0x2b;
         ;    8097 sfrw OCR1A=0x2a;  // 16 bit access
         ;    8098 sfrb TCNT1L=0x2c;
         ;    8099 sfrb TCNT1H=0x2d;
         ;    8100 sfrw TCNT1=0x2c;  // 16 bit access
         ;    8101 sfrb TCCR1B=0x2e;
         ;    8102 sfrb TCCR1A=0x2f;
         ;    8103 sfrb ASSR=0x30;
         ;    8104 sfrb OCR0=0x31;
         ;    8105 sfrb TCNT0=0x32;
         ;    8106 sfrb TCCR0=0x33;
