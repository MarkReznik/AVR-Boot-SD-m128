004642 e2ea      	LDI  R30,LOW(42)
004643   +  	__PUTB1MN _Data,14
004643 93e0 05fb 	STS  _data+0xe,R30
          	.ENDM
         ;   10463           Data[15] = '*'; 
004645   +  	__PUTB1MN _Data,15
004645 93e0 05fc 	STS  _data+0xf,R30
          	.ENDM
         ;   10464      } 
         ;   10465      Data[16] = 0;   
          _0x9EB:
004647 27ee      	CLR  R30
004648   +  	__PUTB1MN _Data,16
004648 93e0 05fd 	STS  _data+0x10,R30
          	.ENDM
         ;   10466      MaxNum = 16;
00464a e1e0      	LDI  R30,LOW(16)
00464b 93e0 05eb 	STS  _MaxNum,R30
         ;   10467 }     
00464d c13b      	RJMP _0xAAA
         ;   10468 char EnterTimer(char Button, char NextCode)      // change timer
         ;   10469 {                             
          _EnterTimer:
         ;   10470        NextCode--;    
00464e 81e8      	LD   R30,Y
00464f 50e1      	SUBI R30,LOW(1)
004650 83e8      	ST   Y,R30
         ;   10471        if(Button == ZERO)Button = 0; 
004651 81a9      	LDD  R26,Y+1
004652 30aa      	CPI  R26,LOW(0xA)
004653 f411      	BRNE _0x9ED
004654 27ee      	CLR  R30
004655 83e9      	STD  Y+1,R30
         ;   10472        if((NextCode >= 0)&&(NextCode <= 6)&&(Button == ALT)){
          _0x9ED:
004656 81a8      	LD   R26,Y
004657 30a0      	CPI  R26,0
004658 f030      	BRLO _0x9EF
004659 e0e6      	LDI  R30,LOW(6)
00465a 17ea      	CP   R30,R26
00465b f018      	BRLO _0x9EF
