         ;    4685 #pragma warn+     
         ;    4686 char eeprom *ptr_to_eeprom;  
         
          	.DSEG
          _ptr_to_eeprom:
00068f      	.BYTE 0x2
         ;    4687 unsigned int  Pointer;                 // pointer to data from eeprom
          _Pointer:
000691      	.BYTE 0x2
         ;    4688 void WriteData(char List)  //write data to eeprom
         ;    4689 {                  
         
          	.CSEG
          _WriteData:
         ;    4690     char i;
         ;    4691     int ind;
         ;    4692     ind = List - 1;
0023ee 940e 4c61 	CALL SUBOPT_0xA4
         ;	List -> Y+3
         ;	i -> R16
         ;	ind -> R17,R18
         ;    4693     ptr_to_eeprom = ptr_EEPROM + Pointer + (ind * MaxNum);
0023f0 93ff      	PUSH R31
0023f1 93ef      	PUSH R30
0023f2 940e 4c6f 	CALL SUBOPT_0xA5
0023f4 91af      	POP  R26
0023f5 91bf      	POP  R27
0023f6 0fea      	ADD  R30,R26
0023f7 1ffb      	ADC  R31,R27
0023f8 93e0 068f 	STS  _ptr_to_eeprom,R30
0023fa 93f0 0690 	STS  _ptr_to_eeprom+1,R31
         ;    4694     for(i = 0; i < MaxNum; i++)        
0023fc e000      	LDI  R16,LOW(0)
          _0x4E0:
0023fd 91e0 05eb 	LDS  R30,_MaxNum
0023ff 170e      	CP   R16,R30
002400 f440      	BRSH _0x4E1
         ;    4695        EEPROM_write(ptr_to_eeprom++, Data[i]);     // wtite to eeprom                                                                                     
002401 940e 4c77 	CALL SUBOPT_0xA6
002403 940e 4af2 	CALL SUBOPT_0x71
002405 93ea      	ST   -Y,R30
002406 d027      	RCALL _EEPROM_write
002407 5f0f      	SUBI R16,-1
002408 cff4      	RJMP _0x4E0
          _0x4E1:
         ;    4696     
         ;    4697 }
002409 940e 50a9 	CALL __LOADLOCR3
00240b c097      	RJMP _0xAB6
         ;    4698 void ReadData(char List)  //read data from eeprom
         ;    4699 {
          _ReadData:
         ;    4700     char i;   
         ;    4701     int ind;
         ;    4702     ind = List - 1;
00240c 940e 4c61 	CALL SUBOPT_0xA4
         ;	List -> Y+3
         ;	i -> R16
         ;	ind -> R17,R18
         ;    4703     ptr_to_eeprom = ptr_EEPROM + Pointer + (ind * MaxNum);
00240e 93ff      	PUSH R31
00240f 93ef      	PUSH R30
002410 940e 4c6f 	CALL SUBOPT_0xA5
002412 91af      	POP  R26
002413 91bf      	POP  R27
002414 0fea      	ADD  R30,R26
002415 1ffb      	ADC  R31,R27
002416 93e0 068f 	STS  _ptr_to_eeprom,R30
002418 93f0 0690 	STS  _ptr_to_eeprom+1,R31
         ;    4704     for(i = 0; i < MaxNum; i++)        
00241a e000      	LDI  R16,LOW(0)
          _0x4E4:
00241b 91e0 05eb 	LDS  R30,_MaxNum
00241d 170e      	CP   R16,R30
00241e f460      	BRSH _0x4E5
         ;    4705        Data[i] = EEPROM_read(ptr_to_eeprom++);     // read from eeprom   
00241f 940e 4c84 	CALL SUBOPT_0xA7
002421 93ff      	PUSH R31
002422 93ef      	PUSH R30
002423 940e 4c77 	CALL SUBOPT_0xA6
002425 d013      	RCALL _EEPROM_read
002426 91af      	POP  R26
002427 91bf      	POP  R27
002428 93ec      	ST   X,R30
002429 5f0f      	SUBI R16,-1
00242a cff0      	RJMP _0x4E4
          _0x4E5:
         ;    4706 } 
00242b 940e 50a9 	CALL __LOADLOCR3
00242d c075      	RJMP _0xAB6
         ;    4707 void EEPROM_write(char eeprom* uiAddress, unsigned char ucData)
         ;    4708 {
          _EEPROM_write:
         ;    4709     // Wait for compleshion of previous write
         ;    4710     while(EECR & (1<<0x02));         // EEWE
          _0x4E7:
00242e 99e2      	SBIC 0x1C,2
00242f cffe      	RJMP _0x4E7
         ;    4711     
         ;    4712     // set up address and data registers
         ;    4713    
         ;    4714     *uiAddress = ucData;
002430 81e8      	LD   R30,Y
002431 81a9      	LDD  R26,Y+1
002432 81ba      	LDD  R27,Y+1+1
002433 940e 508c 	CALL __EEPROMWRB
         ;    4715     
         ;    4716     // Write logical one to EEMWE
         ;    4717     
         ;    4718     EECR|= (1<<0x04);            //EEMWE
002435 9ae4      	SBI  0x1C,4
         ;    4719     
         ;    4720     // Start eeprom write by setting EEWE
         ;    4721     
         ;    4722     EECR|= (1<<0x02);          //EEWE
002436 9ae2      	SBI  0x1C,2
         ;    4723 }
002437 9623      	ADIW R28,3
002438 9508      	RET
         ;    4724 unsigned char EEPROM_read(char eeprom* uiAddress)
         ;    4725 {
          _EEPROM_read:
         ;    4726     // wait for completion of previous write
         ;    4727     while(EECR & (1<<0x02));       // EEWE
          _0x4EB:
002439 99e2      	SBIC 0x1C,2
00243a cffe      	RJMP _0x4EB
         ;    4728        
         ;    4729     // start eeprom read by eriting EERE
         ;    4730     
         ;    4731     EECR|= (1<<0x01);       //EERE
00243b 9ae1      	SBI  0x1C,1
         ;    4732     return (*uiAddress);
00243c 81a8      	LD   R26,Y
00243d 81b9      	LDD  R27,Y+1
00243e 940e 5082 	CALL __EEPROMRDB
002440 9622      	ADIW R28,2
002441 9508      	RET
         ;    4733     
         ;    4734 }    
         ;    4735    
         ;    4736 #include "FuncBit.h"
         ;    4737 unsigned char GetBit(unsigned char Index); // return bit in place Index
         ;    4738 unsigned char GetFirstBit(unsigned char Index);//return number of first bit
         ;    4739 void ClearBit(char* str,char bit);
         ;    4740 void cpyBit(char* str1,char Bit1,char* str2,char Bit2);
         ;    4741 unsigned char ClearFirstBit(unsigned char Key);  //clear first bit from byte
         ;    4742 unsigned char ClearFirstBit(unsigned char Key)  // return byte with out first bit
         ;    4743 {
         ;    4744   unsigned char Index = 1;
         ;    4745   while(!(Key & Index)&&(Index))
         ;	Key -> Y+1
         ;	Index -> R16
         ;    4746        Index = Index * 2;
         ;    4747   Key = Key &~Index;
         ;    4748   return(Key);    
         ;    4749 }
         ;    4750 unsigned char GetFirstBit(unsigned char AllBords)  // return number of first bit
         ;    4751 {
          _GetFirstBit:
         ;    4752   unsigned char Index = 1;
         ;    4753   unsigned char Ind = 0;
         ;    4754   if(AllBords)
002442 931a      	ST   -Y,R17
002443 930a      	ST   -Y,R16
         ;	AllBords -> Y+2
         ;	Index -> R16
         ;	Ind -> R17
002444 e001      	LDI  R16,1
002445 e010      	LDI  R17,0
002446 81ea      	LDD  R30,Y+2
002447 30e0      	CPI  R30,0
002448 f051      	BREQ _0x4FA
         ;    4755   while(!(AllBords & Index)&&(Index))
          _0x4FB:
002449 2fe0      	MOV  R30,R16
00244a 81aa      	LDD  R26,Y+2
00244b 23ea      	AND  R30,R26
00244c f411      	BRNE _0x4FE
00244d 3000      	CPI  R16,0
00244e f409      	BRNE _0x4FF
          _0x4FE:
00244f c003      	RJMP _0x4FD
          _0x4FF:
         ;    4756   {
         ;    4757        Index*= 2;
002450 0f00      	LSL  R16
         ;    4758        Ind++;
002451 5f1f      	SUBI R17,-1
         ;    4759   }    
002452 cff6      	RJMP _0x4FB
          _0x4FD:
         ;    4760   return (Ind);
          _0x4FA:
002453 2fe1      	MOV  R30,R17
002454 8119      	LDD  R17,Y+1
002455 8108      	LDD  R16,Y+0
002456 9623      	ADIW R28,3
002457 9508      	RET
         ;    4761 }
         ;    4762 unsigned char GetBit(unsigned char Index)  // return byte with first bit up
         ;    4763 {   
          _GetBit:
         ;    4764    unsigned char Shift = 0x01;
         ;    4765    Shift = Shift << Index;
002458 930a      	ST   -Y,R16
         ;	Index -> Y+1
         ;	Shift -> R16
002459 e001      	LDI  R16,1
00245a 81e9      	LDD  R30,Y+1
00245b 2fa0      	MOV  R26,R16
00245c 940e 503c 	CALL __LSLB12
00245e 2f0e      	MOV  R16,R30
         ;    4766    return(Shift);
00245f 2fe0      	MOV  R30,R16
002460 8108      	LDD  R16,Y+0
002461 9622      	ADIW R28,2
002462 9508      	RET
         ;    4767 }
         ;    4768 void cpyBit(char* str1,char Bit1,char* str2,char Bit2)
         ;    4769 {
          _cpyBit:
         ;    4770     char ind;
         ;    4771     ClearBit(str1,Bit1);
002463 9721      	SBIW R28,1
         ;	*str1 -> Y+5
         ;	Bit1 -> Y+4
         ;	*str2 -> Y+2
         ;	Bit2 -> Y+1
         ;	ind -> Y+0
002464 81ed      	LDD  R30,Y+5
002465 81fe      	LDD  R31,Y+5+1
002466 93fa      	ST   -Y,R31
002467 93ea      	ST   -Y,R30
002468 81ee      	LDD  R30,Y+6
002469 93ea      	ST   -Y,R30
00246a d020      	RCALL _ClearBit
         ;    4772     for(ind = 0;ind < 16;ind++)    
00246b 27ee      	CLR  R30
00246c 83e8      	ST   Y,R30
          _0x503:
00246d 81a8      	LD   R26,Y
00246e 31a0      	CPI  R26,LOW(0x10)
00246f f4c8      	BRSH _0x504
         ;    4773     
         ;    4774       if(str2[ind] & Bit2)
002470 940e 4c89 	CALL SUBOPT_0xA8
002472 0fae      	ADD  R26,R30
002473 1fbf      	ADC  R27,R31
002474 91ec      	LD   R30,X
002475 81a9      	LDD  R26,Y+1
002476 23ea      	AND  R30,R26
002477 f071      	BREQ _0x505
         ;    4775           str1[ind]|= Bit1;
002478 81e8      	LD   R30,Y
002479 81ad      	LDD  R26,Y+5
00247a 81be      	LDD  R27,Y+5+1
00247b 27ff      	CLR  R31
00247c 0fea      	ADD  R30,R26
00247d 1ffb      	ADC  R31,R27
00247e 93ff      	PUSH R31
00247f 93ef      	PUSH R30
002480 81e0      	LD   R30,Z
002481 81ac      	LDD  R26,Y+4
002482 2bea      	OR   R30,R26
002483 91af      	POP  R26
002484 91bf      	POP  R27
002485 93ec      	ST   X,R30
         ;    4776        
         ;    4777 } 
          _0x505:
002486 940e 4b58 	CALL SUBOPT_0x80
002488 cfe4      	RJMP _0x503
          _0x504:
002489 9627      	ADIW R28,7
00248a 9508      	RET
         ;    4778 void ClearBit(char* str,char bit1)
         ;    4779 {
          _ClearBit:
         ;    4780     char ind;
         ;    4781     for(ind = 0;ind < 16;ind++)
00248b 9721      	SBIW R28,1
         ;	*str -> Y+2
         ;	bit1 -> Y+1
         ;	ind -> Y+0
00248c 27ee      	CLR  R30
00248d 83e8      	ST   Y,R30
          _0x508:
00248e 81a8      	LD   R26,Y
00248f 31a0      	CPI  R26,LOW(0x10)
002490 f490      	BRSH _0x509
         ;    4782       str[ind]&= ~bit1;
002491 940e 4c89 	CALL SUBOPT_0xA8
002493 0fea      	ADD  R30,R26
002494 1ffb      	ADC  R31,R27
002495 93ff      	PUSH R31
002496 93ef      	PUSH R30
002497 81e0      	LD   R30,Z
002498 93ef      	PUSH R30
002499 81e9      	LDD  R30,Y+1
00249a 95e0      	COM  R30
00249b 91af      	POP  R26
00249c 23ea      	AND  R30,R26
00249d 91af      	POP  R26
00249e 91bf      	POP  R27
00249f 93ec      	ST   X,R30
0024a0 940e 4b58 	CALL SUBOPT_0x80
0024a2 cfeb      	RJMP _0x508
          _0x509:
         ;    4783 }
          _0xAB6:
0024a3 9624      	ADIW R28,4
0024a4 9508      	RET
         ;    4784 #include <mega128.h> 
         ;    4785 // CodeVisionAVR C Compiler
         ;    4786 // (C) 1998-2001 Pavel Haiduc, HP InfoTech S.R.L.
         ;    4787 
         ;    4788 // I/O registers definitions for the ATmega128
         ;    4789 
         ;    4790 #ifndef _MEGA128_INCLUDED_
         ;    4791 #define _MEGA128_INCLUDED_
         ;    4792 
         ;    4793 #pragma used+
         ;    4794 sfrb PINF=0;
         ;    4795 sfrb PINE=1;
         ;    4796 sfrb DDRE=2;
         ;    4797 sfrb PORTE=3;
         ;    4798 sfrb ADCL=4;
         ;    4799 sfrb ADCH=5;
         ;    4800 sfrw ADCW=4;      // 16 bit access
         ;    4801 sfrb ADCSRA=6;
         ;    4802 sfrb ADMUX=7;
         ;    4803 sfrb ACSR=8;
         ;    4804 sfrb UBRR0L=9;
         ;    4805 sfrb UCSR0B=0xa;
         ;    4806 sfrb UCSR0A=0xb;
         ;    4807 sfrb UDR0=0xc;
         ;    4808 sfrb SPCR=0xd;
         ;    4809 sfrb SPSR=0xe;
         ;    4810 sfrb SPDR=0xf;
         ;    4811 sfrb PIND=0x10;
         ;    4812 sfrb DDRD=0x11;
         ;    4813 sfrb PORTD=0x12;
         ;    4814 sfrb PINC=0x13;
         ;    4815 sfrb DDRC=0x14;
         ;    4816 sfrb PORTC=0x15;
         ;    4817 sfrb PINB=0x16;
         ;    4818 sfrb DDRB=0x17;
         ;    4819 sfrb PORTB=0x18;
         ;    4820 sfrb PINA=0x19;
         ;    4821 sfrb DDRA=0x1a;
         ;    4822 sfrb PORTA=0x1b;
         ;    4823 sfrb EECR=0x1c;
         ;    4824 sfrb EEDR=0x1d;
         ;    4825 sfrb EEARL=0x1e;
         ;    4826 sfrb EEARH=0x1f;
         ;    4827 sfrw EEAR=0x1e;   // 16 bit access
         ;    4828 sfrb SFIOR=0x20;
         ;    4829 sfrb WDTCR=0x21;
         ;    4830 sfrb OCDR=0x22;
         ;    4831 sfrb OCR2=0x23;
         ;    4832 sfrb TCNT2=0x24;
         ;    4833 sfrb TCCR2=0x25;
         ;    4834 sfrb ICR1L=0x26;
