         ;    8041 // (C) 1998-2001 Pavel Haiduc, HP InfoTech S.R.L.
         ;    8042 
         ;    8043 // I/O registers definitions for the ATmega128
         ;    8044 
         ;    8045 #ifndef _MEGA128_INCLUDED_
         ;    8046 #define _MEGA128_INCLUDED_
         ;    8047 
         ;    8048 #pragma used+
         ;    8049 sfrb PINF=0;
         ;    8050 sfrb PINE=1;
         ;    8051 sfrb DDRE=2;
         ;    8052 sfrb PORTE=3;
         ;    8053 sfrb ADCL=4;
         ;    8054 sfrb ADCH=5;
         ;    8055 sfrw ADCW=4;      // 16 bit access
         ;    8056 sfrb ADCSRA=6;
         ;    8057 sfrb ADMUX=7;
         ;    8058 sfrb ACSR=8;
         ;    8059 sfrb UBRR0L=9;
         ;    8060 sfrb UCSR0B=0xa;
         ;    8061 sfrb UCSR0A=0xb;
         ;    8062 sfrb UDR0=0xc;
         ;    8063 sfrb SPCR=0xd;
         ;    8064 sfrb SPSR=0xe;
         ;    8065 sfrb SPDR=0xf;
         ;    8066 sfrb PIND=0x10;
         ;    8067 sfrb DDRD=0x11;
         ;    8068 sfrb PORTD=0x12;
         ;    8069 sfrb PINC=0x13;
         ;    8070 sfrb DDRC=0x14;
         ;    8071 sfrb PORTC=0x15;
