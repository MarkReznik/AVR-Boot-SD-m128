         ;   10123 
         ;   10124 void ReadY_N(void)            // return y/n ques. from eeprom(Y/N QUES.)
         ;   10125 {
          _ReadY_N:
         ;   10126    char ind;
         ;   10127    StatusY_N = StatusY_N2 = 0;
004519 930a      	ST   -Y,R16
         ;	ind -> R16
00451a 27ee      	CLR  R30
00451b 27ff      	CLR  R31
00451c 93e0 0641 	STS  _StatusY_N2,R30
00451e 93f0 0642 	STS  _StatusY_N2+1,R31
004520 93e0 063f 	STS  _StatusY_N,R30
004522 93f0 0640 	STS  _StatusY_N+1,R31
         ;   10128    Pointer = ZoneYesNo;
004524 e9e8      	LDI  R30,LOW(664)
004525 e0f2      	LDI  R31,HIGH(664)
004526 940e 4966 	CALL SUBOPT_0x3D
         ;   10129    MaxNum = 16;
         ;   10130    ReadData(1);
004528 940e 4ba4 	CALL SUBOPT_0x8D
         ;   10131    for(ind = 0;ind<16;ind++)
00452a e000      	LDI  R16,LOW(0)
          _0x9C8:
00452b 3100      	CPI  R16,16
00452c f4a8      	BRSH _0x9C9
         ;   10132      if(Data[ind]&0x0F)
00452d 940e 4af2 	CALL SUBOPT_0x71
00452f 70ef      	ANDI R30,LOW(0xF)
004530 f079      	BREQ _0x9CA
         ;   10133          StatusY_N|= 0x01 << ind;
004531 940e 4e95 	CALL SUBOPT_0xFC
004533 91a0 063f 	LDS  R26,_StatusY_N
004535 91b0 0640 	LDS  R27,_StatusY_N+1
004537 27ff      	CLR  R31
004538 fde7      	SBRC R30,7
004539 efff      	SER  R31
00453a 2bea      	OR   R30,R26
00453b 2bfb      	OR   R31,R27
00453c 93e0 063f 	STS  _StatusY_N,R30
00453e 93f0 0640 	STS  _StatusY_N+1,R31
         ;   10134    ReadData(2);              
          _0x9CA:
004540 5f0f      	SUBI R16,-1
004541 cfe9      	RJMP _0x9C8
          _0x9C9:
004542 e0e2      	LDI  R30,LOW(2)
004543 93ea      	ST   -Y,R30
004544 940e 240c 	CALL _ReadData
         ;   10135    for(ind = 0;ind<16;ind++)
004546 e000      	LDI  R16,LOW(0)
          _0x9CC:
004547 3100      	CPI  R16,16
004548 f478      	BRSH _0x9CD
         ;   10136      if(Data[ind]&0x0F)
004549 940e 4af2 	CALL SUBOPT_0x71
00454b 70ef      	ANDI R30,LOW(0xF)
00454c f049      	BREQ _0x9CE
         ;   10137          StatusY_N2|= 0x01 << ind;           
00454d 2fe0      	MOV  R30,R16
00454e 940e 4b18 	CALL SUBOPT_0x78
004550 2bea      	OR   R30,R26
004551 2bfb      	OR   R31,R27
004552 93e0 0641 	STS  _StatusY_N2,R30
004554 93f0 0642 	STS  _StatusY_N2+1,R31
         ;   10138 }
          _0x9CE:
004556 5f0f      	SUBI R16,-1
004557 cfef      	RJMP _0x9CC
          _0x9CD:
          _0xAAC:
004558 9109      	LD   R16,Y+
004559 9508      	RET
