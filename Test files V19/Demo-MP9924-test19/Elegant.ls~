         ;      68 #ifdef _DEBUG_TERMINAL_IO_
         ;      69 // Used for AVR Studio Terminal I/O
         ;      70 // DON'T CHANGE!
         ;      71 #pragma warn-
         ;      72 #pragma keep+
         ;      73 void _c_getchar(void) {}
         ;      74 
         ;      75 char getchar(void)
