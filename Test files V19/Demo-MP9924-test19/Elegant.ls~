001fd7 940e 4bbf 	CALL SUBOPT_0x92
001fd9 93ef      	PUSH R30
001fda 81e8      	LD   R30,Y
001fdb 940e 4b78 	CALL SUBOPT_0x86
001fdd 70ef      	ANDI R30,LOW(0xF)
001fde 91af      	POP  R26
001fdf 0fea      	ADD  R30,R26
001fe0 c010      	RJMP _0xAB7
         ;    4015     else   
