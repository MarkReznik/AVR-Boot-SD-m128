         ;   10053 void WriteValueCycle(void)        // Write cycle of sirens
         ;   10054 {   
          _WriteValueCycle:
         ;   10055     Pointer = Status_SAC;
0044a4 e1e8      	LDI  R30,LOW(536)
0044a5 e0f2      	LDI  R31,HIGH(536)
0044a6 940e 4966 	CALL SUBOPT_0x3D
         ;   10056     MaxNum = 16;
         ;   10057     ReadData(1);
0044a8 940e 4ba4 	CALL SUBOPT_0x8D
         ;   10058     Data[6] = ValueCycles;
0044aa   +  	__POINTW2MN _Data,6
0044aa efa3      	LDI  R26,LOW(_data+0x6)
0044ab e0b5      	LDI  R27,HIGH(_data+0x6)
          	.ENDM
0044ac 91e0 064a 	LDS  R30,_ValueCycles
0044ae 93ec      	ST   X,R30
         ;   10059     WriteData(1);    
0044af 940e 4bed 	CALL SUBOPT_0x98
         ;   10060 }
0044b1 9508      	RET
         ;   10061 void ReadValueCycle(void)        // Read cycle of sirens
         ;   10062 {     
         ;   10063     Pointer = Status_SAC;
         ;   10064     MaxNum = 16;
         ;   10065     ReadData(1);
         ;   10066     ValueCycles = Data[6];
         ;   10067 }  
         ;   10068 void WriteActivate(void)        // write bit of Activate
         ;   10069 {     
          _WriteActivate:
         ;   10070     char i;
         ;   10071     Pointer = Status_SAC;
0044b2 940e 4f89 	CALL SUBOPT_0x123
         ;	i -> R16
         ;   10072     MaxNum = 16;
         ;   10073     ReadData(1); 
0044b4 940e 4ba4 	CALL SUBOPT_0x8D
         ;   10074     Data[1] = Data[2] = 0; 
0044b6 27ee      	CLR  R30
0044b7   +  	__PUTB1MN _Data,2
0044b7 93e0 05ef 	STS  _data+0x2,R30
          	.ENDM
0044b9   +  	__PUTB1MN _Data,1
0044b9 93e0 05ee 	STS  _data+0x1,R30
          	.ENDM
         ;   10075     for(i = 0;i < 8;i++){
0044bb e000      	LDI  R16,LOW(0)
          _0x9B0:
0044bc 3008      	CPI  R16,8
0044bd f540      	BRSH _0x9B1
         ;   10076          if(Zone[i] & 0x10)
0044be 940e 4961 	CALL SUBOPT_0x3C
0044c0 81e0      	LD   R30,Z
0044c1 71e0      	ANDI R30,LOW(0x10)
0044c2 f069      	BREQ _0x9B2
         ;   10077              Data[1]|= (0x01<<i);  
0044c3   +  	__POINTW1MN _Data,1
0044c3 eeee      	LDI  R30,LOW(_data+0x1)
0044c4 e0f5      	LDI  R31,HIGH(_data+0x1)
          	.ENDM
0044c5 93ff      	PUSH R31
0044c6 93ef      	PUSH R30
0044c7 81e0      	LD   R30,Z
0044c8 93ef      	PUSH R30
0044c9 940e 4e95 	CALL SUBOPT_0xFC
0044cb 91af      	POP  R26
0044cc 2bea      	OR   R30,R26
0044cd 91af      	POP  R26
0044ce 91bf      	POP  R27
0044cf 93ec      	ST   X,R30
         ;   10078          if(Zone[i+8] & 0x10)
          _0x9B2:
0044d0 940e 4f8d 	CALL SUBOPT_0x124
0044d2 55e1      	SUBI R30,LOW(-_Zone)
0044d3 4ff8      	SBCI R31,HIGH(-_Zone)
0044d4 81e0      	LD   R30,Z
0044d5 71e0      	ANDI R30,LOW(0x10)
0044d6 f069      	BREQ _0x9B3
         ;   10079              Data[2]|= (0x01<<i);         
0044d7   +  	__POINTW1MN _Data,2
0044d7 eeef      	LDI  R30,LOW(_data+0x2)
0044d8 e0f5      	LDI  R31,HIGH(_data+0x2)
          	.ENDM
0044d9 93ff      	PUSH R31
0044da 93ef      	PUSH R30
0044db 81e0      	LD   R30,Z
0044dc 93ef      	PUSH R30
0044dd 940e 4e95 	CALL SUBOPT_0xFC
0044df 91af      	POP  R26
0044e0 2bea      	OR   R30,R26
0044e1 91af      	POP  R26
0044e2 91bf      	POP  R27
0044e3 93ec      	ST   X,R30
         ;   10080     }         
