         ;   10023 void WriteStatus_SAC(void)    // Read status of Stay & Away to eeprom
         ;   10024 {
          _WriteStatus_SAC:
         ;   10025     Pointer = Status_SAC; 
004473 e1e8      	LDI  R30,LOW(536)
004474 e0f2      	LDI  R31,HIGH(536)
004475 940e 4966 	CALL SUBOPT_0x3D
         ;   10026     MaxNum = 16;
         ;   10027     ReadData(1);
004477 940e 4ba4 	CALL SUBOPT_0x8D
         ;   10028     Data[0] = ButtonChime;
