         ;     119 // CodeVisionAVR C Compiler
         ;     120 // (C) 1998-2001 Pavel Haiduc, HP InfoTech S.R.L.
         ;     121 
         ;     122 // I/O registers definitions for the ATmega128
         ;     123 
         ;     124 #ifndef _MEGA128_INCLUDED_
         ;     125 #define _MEGA128_INCLUDED_
         ;     126 
         ;     127 #pragma used+
         ;     128 sfrb PINF=0;
         ;     129 sfrb PINE=1;
         ;     130 sfrb DDRE=2;
         ;     131 sfrb PORTE=3;
         ;     132 sfrb ADCL=4;
         ;     133 sfrb ADCH=5;
         ;     134 sfrw ADCW=4;      // 16 bit access
         ;     135 sfrb ADCSRA=6;
         ;     136 sfrb ADMUX=7;
         ;     137 sfrb ACSR=8;
         ;     138 sfrb UBRR0L=9;
         ;     139 sfrb UCSR0B=0xa;
         ;     140 sfrb UCSR0A=0xb;
         ;     141 sfrb UDR0=0xc;
         ;     142 sfrb SPCR=0xd;
         ;     143 sfrb SPSR=0xe;
         ;     144 sfrb SPDR=0xf;
         ;     145 sfrb PIND=0x10;
         ;     146 sfrb DDRD=0x11;
         ;     147 sfrb PORTD=0x12;
         ;     148 sfrb PINC=0x13;
         ;     149 sfrb DDRC=0x14;
         ;     150 sfrb PORTC=0x15;
         ;     151 sfrb PINB=0x16;
         ;     152 sfrb DDRB=0x17;
         ;     153 sfrb PORTB=0x18;
         ;     154 sfrb PINA=0x19;
         ;     155 sfrb DDRA=0x1a;
         ;     156 sfrb PORTA=0x1b;
         ;     157 sfrb EECR=0x1c;
         ;     158 sfrb EEDR=0x1d;
         ;     159 sfrb EEARL=0x1e;
         ;     160 sfrb EEARH=0x1f;
         ;     161 sfrw EEAR=0x1e;   // 16 bit access
         ;     162 sfrb SFIOR=0x20;
         ;     163 sfrb WDTCR=0x21;
         ;     164 sfrb OCDR=0x22;
         ;     165 sfrb OCR2=0x23;
         ;     166 sfrb TCNT2=0x24;
         ;     167 sfrb TCCR2=0x25;
