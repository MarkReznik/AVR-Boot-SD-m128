          _BitPauseSpike:
000796      	.BYTE 0x1
         ;    5332 unsigned char StatusElement;          
          _StatusElement:
000797      	.BYTE 0x1
         ;    5333 unsigned char BaundPhone;
          _BaundPhone:
000798      	.BYTE 0x1
         ;    5334 unsigned char SpikeKeybord, BordSpiker,SaveBordSpiker,SpikePuls; 
          _SpikeKeybord:
000799      	.BYTE 0x1
          _BordSpiker:
00079a      	.BYTE 0x1
          _SaveBordSpiker:
00079b      	.BYTE 0x1
          _SpikePuls:
00079c      	.BYTE 0x1
         ;    5335 unsigned char MessageOut;
          _MessageOut:
00079d      	.BYTE 0x1
         ;    5336 void SpikeIn(void)
         ;    5337 {                
         
          	.CSEG
          _SpikeIn:
         ;    5338     char j;      
         ;    5339     if(SpikePuls){     
0024a5 930a      	ST   -Y,R16
         ;	j -> R16
0024a6 91e0 079c 	LDS  R30,_SpikePuls
0024a8 30e0      	CPI  R30,0
0024a9 f0b9      	BREQ _0x521
         ;    5340               if(!(PINC & 0x10)){   // if end of get message  
0024aa 999c      	SBIC 0x13,4
0024ab c014      	RJMP _0x522
         ;    5341                         if(!pStrSpike[nStr]){  
0024ac 940e 4c8e 	CALL SUBOPT_0xA9
0024ae 30e0      	CPI  R30,0
0024af f461      	BRNE _0x523
         ;    5342                                    StopMessage();   
0024b0 d1e1      	RCALL _StopMessage
         ;    5343                                    if(SpikePuls == 2)  
0024b1 91a0 079c 	LDS  R26,_SpikePuls
0024b3 30a2      	CPI  R26,LOW(0x2)
0024b4 f421      	BRNE _0x524
         ;    5344                                           SpikePuls = 0; 
0024b5 27ee      	CLR  R30
0024b6 93e0 079c 	STS  _SpikePuls,R30
         ;    5345                                    else
0024b8 c002      	RJMP _0x525
          _0x524:
         ;    5346                                           SpikeStatusPuls(MessageOut);                                                                                                                       
0024b9 940e 4c95 	CALL SUBOPT_0xAA
          _0x525:
         ;    5347                         }             
         ;    5348                         else{                                                                
0024bb c004      	RJMP _0x526
          _0x523:
         ;    5349                                    Spiker(pStrSpike[nStr]);        
0024bc 940e 4c8e 	CALL SUBOPT_0xA9
0024be 940e 4c9a 	CALL SUBOPT_0xAB
         ;    5350                                    nStr++;                                                                                                        
         ;    5351                         }                                                                                  
          _0x526:
         ;    5352               }                                                                               
         ;    5353     }else     
          _0x522:
0024c0 c199      	RJMP _0x527
          _0x521:
         ;    5354     if(SpikeKeybord){     
0024c1 91e0 0799 	LDS  R30,_SpikeKeybord
0024c3 30e0      	CPI  R30,0
0024c4 f101      	BREQ _0x528
         ;    5355               if(!(PINC & 0x10)){   // if end of get message  
0024c5 999c      	SBIC 0x13,4
0024c6 c01d      	RJMP _0x529
         ;    5356                         if(!pStrSpike[nStr]){  
0024c7 940e 4c8e 	CALL SUBOPT_0xA9
0024c9 30e0      	CPI  R30,0
0024ca f4a9      	BRNE _0x52A
         ;    5357                                    StopMessage();     
0024cb d1c6      	RCALL _StopMessage
         ;    5358                                    SpikeKeybord = 0;                                      
0024cc 27ee      	CLR  R30
0024cd 93e0 0799 	STS  _SpikeKeybord,R30
         ;    5359                                    RealTime[7] = AllBords;   
0024cf   +  	__POINTW2MN _RealTime,7
0024cf e8ad      	LDI  R26,LOW(_realtime+0x7)
0024d0 e0b5      	LDI  R27,HIGH(_realtime+0x7)
          	.ENDM
0024d1 940e 484d 	CALL SUBOPT_0x13
         ;    5360                                    BordSpiker = 0;                                   
0024d3 93e0 079a 	STS  _BordSpiker,R30
         ;    5361                                    if(PhoneUp){  
0024d5 91e0 0780 	LDS  R30,_PhoneUp
0024d7 30e0      	CPI  R30,0
0024d8 f031      	BREQ _0x52B
         ;    5362                                              PORTD&= ~0x40;
0024d9 940e 4bb8 	CALL SUBOPT_0x91
0024db 940e 4ca3 	CALL SUBOPT_0xAC
         ;    5363                                              BitPauseSpike = 1;  
0024dd 93e0 0796 	STS  _BitPauseSpike,R30
         ;    5364                                    }          
         ;    5365                         }             
          _0x52B:
         ;    5366                         else{                                                                
0024df c004      	RJMP _0x52C
          _0x52A:
         ;    5367                                    Spiker(pStrSpike[nStr]);        
0024e0 940e 4c8e 	CALL SUBOPT_0xA9
0024e2 940e 4c9a 	CALL SUBOPT_0xAB
         ;    5368                                    nStr++;                                                                                                        
         ;    5369                         }                                                                                  
          _0x52C:
         ;    5370               }                                                                               
         ;    5371     }else     
          _0x529:
0024e4 c175      	RJMP _0x52D
          _0x528:
         ;    5372     if(!(PINC & 0x08)){    
0024e5 999b      	SBIC 0x13,3
0024e6 c146      	RJMP _0x52E
         ;    5373         if(!RealTime[7]){    
0024e7   +  	__GETB1MN _RealTime,7
0024e7 91e0 058d 	LDS  R30,_realtime+0x7
          	.ENDM
0024e9 30e0      	CPI  R30,0
0024ea f011      	BREQ PC+3
0024eb 940c 262c 	JMP _0x52F
         ;    5374                  SpikeKeybord = 0;     
0024ed 27ee      	CLR  R30
0024ee 93e0 0799 	STS  _SpikeKeybord,R30
         ;    5375                  BaundPhone = 0; 
0024f0 93e0 0798 	STS  _BaundPhone,R30
         ;    5376                  if(StatusArm != SaveStatusArm){ 
0024f2 91e0 0794 	LDS  R30,_SaveStatusArm
0024f4 91a0 0658 	LDS  R26,_StatusArm
0024f6 17ea      	CP   R30,R26
0024f7 f031      	BREQ _0x530
         ;    5377                       SaveStatusArm = StatusArm; 
0024f8 91e0 0658 	LDS  R30,_StatusArm
0024fa 93e0 0794 	STS  _SaveStatusArm,R30
         ;    5378                       StopMessage(); 
0024fc 940e 4cac 	CALL SUBOPT_0xAD
         ;    5379                       tSpikeSystem(StrInt); 
         ;    5380                  }      
         ;    5381                  if(!Status && SaveStatus){ 
          _0x530:
0024fe 91e0 05d9 	LDS  R30,_Status
002500 30e0      	CPI  R30,0
002501 f421      	BRNE _0x532
002502 91e0 0793 	LDS  R30,_SaveStatus
002504 30e0      	CPI  R30,0
002505 f409      	BRNE _0x533
          _0x532:
002506 c006      	RJMP _0x531
          _0x533:
         ;    5382                       SaveStatus = Status; 
002507 91e0 05d9 	LDS  R30,_Status
002509 93e0 0793 	STS  _SaveStatus,R30
         ;    5383                       StopMessage(); 
00250b 940e 4cac 	CALL SUBOPT_0xAD
         ;    5384                       tSpikeSystem(StrInt); 
         ;    5385                  }               
         ;    5386                  if((StatusSpike == 1)&&(!TimeSpike))
          _0x531:
00250d 91a0 0781 	LDS  R26,_StatusSpike
00250f 30a1      	CPI  R26,LOW(0x1)
002510 f421      	BRNE _0x535
002511 91e0 0782 	LDS  R30,_TimeSpike
002513 30e0      	CPI  R30,0
002514 f009      	BREQ _0x536
          _0x535:
002515 c00d      	RJMP _0x534
          _0x536:
         ;    5387                  {       
         ;    5388                        StopMessage(); 
002516 d17b      	RCALL _StopMessage
         ;    5389                        EntStatus = 1; 
002517 e0e1      	LDI  R30,LOW(1)
002518 93e0 077b 	STS  _EntStatus,R30
         ;    5390                        PauseSpike = 10;          
00251a e0ea      	LDI  R30,LOW(10)
00251b 93e0 0784 	STS  _PauseSpike,R30
         ;    5391                        while(PauseSpike);     
          _0x537:
00251d 91e0 0784 	LDS  R30,_PauseSpike
00251f 30e0      	CPI  R30,0
002520 f7e1      	BRNE _0x537
         ;    5392                        tSpikeSystem(StrInt); 
002521 940e 4cb4 	CALL SUBOPT_0xAE
         ;    5393                  }
         ;    5394                  if(!PhoneUp){   
          _0x534:
002523 91e0 0780 	LDS  R30,_PhoneUp
002525 30e0      	CPI  R30,0
002526 f441      	BRNE _0x53A
         ;    5395                        PORTD&= ~0x40;
002527 940e 4bb8 	CALL SUBOPT_0x91
002529 940e 4ca3 	CALL SUBOPT_0xAC
         ;    5396                        PhoneUp = 1;       
00252b 93e0 0780 	STS  _PhoneUp,R30
         ;    5397                        BitPauseSpike = 0;                  
00252d 940e 4cba 	CALL SUBOPT_0xAF
         ;    5398                        tSpikeSystem(StrInt);  
         ;    5399                  }   
         ;    5400                  if((BitPauseSpike)&&(!CountPauseSpike)){   
          _0x53A:
00252f 91e0 0796 	LDS  R30,_BitPauseSpike
002531 30e0      	CPI  R30,0
002532 f021      	BREQ _0x53C
002533 91e0 0795 	LDS  R30,_CountPauseSpike
002535 30e0      	CPI  R30,0
002536 f009      	BREQ _0x53D
          _0x53C:
002537 c002      	RJMP _0x53B
          _0x53D:
         ;    5401                        BitPauseSpike = 0;                          
002538 940e 4cba 	CALL SUBOPT_0xAF
         ;    5402                        tSpikeSystem(StrInt);  
         ;    5403                  }     
         ;    5404                  if(PINA & 0x80)                      // if detected receiving from moked or telephone
          _0x53B:
00253a 9bcf      	SBIS 0x19,7
00253b c043      	RJMP _0x53E
         ;    5405                  {     
         ;    5406                       if(!NextKey){                                       
00253c 91e0 0785 	LDS  R30,_NextKey
00253e 30e0      	CPI  R30,0
00253f f011      	BREQ PC+3
002540 940c 257e 	JMP _0x53F
         ;    5407                              NextKey = 1;
002542 e0e1      	LDI  R30,LOW(1)
002543 93e0 0785 	STS  _NextKey,R30
         ;    5408                              //PORTA|= 0x40; 
         ;    5409                              //#asm("NOP");    
         ;    5410                              TelTone = PINA & 0x0F;   
002545 b3e9      	IN   R30,0x19
002546 70ef      	ANDI R30,LOW(0xF)
002547 93e0 0783 	STS  _TelTone,R30
         ;    5411                              if(TelTone == 0) TelTone = 10;      
002549 30e0      	CPI  R30,0
00254a f419      	BRNE _0x540
00254b e0ea      	LDI  R30,LOW(10)
00254c 93e0 0783 	STS  _TelTone,R30
         ;    5412                              if(TelTone == 11) TelTone = '*';     
          _0x540:
00254e 91a0 0783 	LDS  R26,_TelTone
002550 30ab      	CPI  R26,LOW(0xB)
002551 f419      	BRNE _0x541
002552 e2ea      	LDI  R30,LOW(42)
002553 93e0 0783 	STS  _TelTone,R30
         ;    5413                              if(TelTone == 12) TelTone = '#';  
          _0x541:
002555 91a0 0783 	LDS  R26,_TelTone
002557 30ac      	CPI  R26,LOW(0xC)
002558 f419      	BRNE _0x542
002559 e2e3      	LDI  R30,LOW(35)
00255a 93e0 0783 	STS  _TelTone,R30
         ;    5414                              if(((TelTone > 0)&&(TelTone <= 10))||(TelTone == '#')||(TelTone == '*'))          // if conect to telephon 
          _0x542:
00255c 91a0 0783 	LDS  R26,_TelTone
00255e 27ee      	CLR  R30
00255f 17ea      	CP   R30,R26
002560 f418      	BRSH _0x544
002561 e0ea      	LDI  R30,LOW(10)
002562 17ea      	CP   R30,R26
002563 f430      	BRSH _0x546
          _0x544:
002564 91a0 0783 	LDS  R26,_TelTone
002566 32a3      	CPI  R26,LOW(0x23)
002567 f011      	BREQ _0x546
002568 32aa      	CPI  R26,LOW(0x2A)
002569 f4a1      	BRNE _0x543
          _0x546:
         ;    5415                              { 
         ;    5416                                       TelButton[TelStop] = TelTone;
00256a 91a0 0792 	LDS  R26,_TelStop
00256c 27bb      	CLR  R27
00256d 57aa      	SUBI R26,LOW(-_TelButton)
00256e 4fb8      	SBCI R27,HIGH(-_TelButton)
00256f 91e0 0783 	LDS  R30,_TelTone
002571 93ec      	ST   X,R30
         ;    5417                                       TelStop++;
002572 91e0 0792 	LDS  R30,_TelStop
002574 5fef      	SUBI R30,-LOW(1)
002575 93e0 0792 	STS  _TelStop,R30
         ;    5418                                       if(TelStop >= 10)
002577 91a0 0792 	LDS  R26,_TelStop
002579 30aa      	CPI  R26,LOW(0xA)
00257a f018      	BRLO _0x548
         ;    5419                                           TelStop = 0; 
00257b 27ee      	CLR  R30
00257c 93e0 0792 	STS  _TelStop,R30
         ;    5420                              }                                                                                                                                        
          _0x548:
         ;    5421                       } 
          _0x543:
         ;    5422                  }
          _0x53F:
         ;    5423                  else{
00257e c0ad      	RJMP _0x549
          _0x53E:
         ;    5424                      NextKey = 0;                        
00257f 27ee      	CLR  R30
002580 93e0 0785 	STS  _NextKey,R30
         ;    5425                      if(TelStop != TelStart)          // if conect to telephon 
002582 91e0 0791 	LDS  R30,_TelStart
002584 91a0 0792 	LDS  R26,_TelStop
002586 17ea      	CP   R30,R26
002587 f0c9      	BREQ _0x54A
         ;    5426                      {                                                             
         ;    5427                              ButtonSpike(TelButton[TelStart]);
002588 27ff      	CLR  R31
002589 57ea      	SUBI R30,LOW(-_TelButton)
00258a 4ff8      	SBCI R31,HIGH(-_TelButton)
00258b 81e0      	LD   R30,Z
00258c 93ea      	ST   -Y,R30
00258d d253      	RCALL _ButtonSpike
         ;    5428                              TelStart++;
00258e 91e0 0791 	LDS  R30,_TelStart
002590 5fef      	SUBI R30,-LOW(1)
002591 93e0 0791 	STS  _TelStart,R30
         ;    5429                              if(TelStart >= 10)
002593 91a0 0791 	LDS  R26,_TelStart
002595 30aa      	CPI  R26,LOW(0xA)
