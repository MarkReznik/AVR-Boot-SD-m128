         ;   10492                if(Button < 6){
0046a3 81a9      	LDD  R26,Y+1
0046a4 30a6      	CPI  R26,LOW(0x6)
0046a5 f458      	BRSH _0x9FF
         ;   10493                        Data[NextCode] = Button + 0x30;
0046a6 940e 4fa9 	CALL SUBOPT_0x12A
0046a8 93ff      	PUSH R31
0046a9 93ef      	PUSH R30
0046aa 81e9      	LDD  R30,Y+1
0046ab 5de0      	SUBI R30,-LOW(48)
0046ac 91af      	POP  R26
0046ad 91bf      	POP  R27
0046ae 93ec      	ST   X,R30
         ;   10494                        NextCode++;
0046af 940e 4b58 	CALL SUBOPT_0x80
         ;   10495                }
         ;   10496        }else
          _0x9FF:
0046b1 c046      	RJMP _0xA00
          _0x9FE:
         ;   10497        if(NextCode == 12){
0046b2 81a8      	LD   R26,Y
0046b3 30ac      	CPI  R26,LOW(0xC)
0046b4 f479      	BRNE _0xA01
         ;   10498                if(Button <= 9){
0046b5 940e 4f76 	CALL SUBOPT_0x120
0046b7 f058      	BRLO _0xA02
         ;   10499                        Data[NextCode] = Button + 0x30;   
0046b8 940e 4fa9 	CALL SUBOPT_0x12A
0046ba 93ff      	PUSH R31
0046bb 93ef      	PUSH R30
0046bc 81e9      	LDD  R30,Y+1
0046bd 5de0      	SUBI R30,-LOW(48)
0046be 91af      	POP  R26
0046bf 91bf      	POP  R27
0046c0 93ec      	ST   X,R30
         ;   10500                        NextCode++;
0046c1 940e 4b58 	CALL SUBOPT_0x80
         ;   10501                }                       
         ;   10502        }else
          _0xA02:
0046c3 c034      	RJMP _0xA03
          _0xA01:
         ;   10503        if(NextCode == 14){
0046c4 81a8      	LD   R26,Y
0046c5 30ae      	CPI  R26,LOW(0xE)
0046c6 f4f1      	BRNE _0xA04
         ;   10504                if(Button == STAY){
0046c7 81a9      	LDD  R26,Y+1
0046c8 30ad      	CPI  R26,LOW(0xD)
0046c9 f439      	BRNE _0xA05
         ;   10505                        Data[NextCode] = 'S';
0046ca 940e 4b61 	CALL SUBOPT_0x82
0046cc e5e3      	LDI  R30,LOW(83)
0046cd 93ec      	ST   X,R30
         ;   10506                        NextCode++;
0046ce 940e 4b58 	CALL SUBOPT_0x80
         ;   10507                }else
0046d0 c013      	RJMP _0xA06
          _0xA05:
         ;   10508                if(Button == AWAY){
0046d1 81a9      	LDD  R26,Y+1
0046d2 30ae      	CPI  R26,LOW(0xE)
0046d3 f439      	BRNE _0xA07
         ;   10509                        Data[NextCode] = 'A';
0046d4 940e 4b61 	CALL SUBOPT_0x82
0046d6 e4e1      	LDI  R30,LOW(65)
0046d7 93ec      	ST   X,R30
         ;   10510                        NextCode++;
0046d8 940e 4b58 	CALL SUBOPT_0x80
         ;   10511                }else
0046da c009      	RJMP _0xA08
          _0xA07:
         ;   10512                if(Button == ALT){
0046db 81a9      	LDD  R26,Y+1
0046dc 30ab      	CPI  R26,LOW(0xB)
0046dd f431      	BRNE _0xA09
         ;   10513                        Data[NextCode] = 'D';
0046de 940e 4b61 	CALL SUBOPT_0x82
0046e0 e4e4      	LDI  R30,LOW(68)
0046e1 93ec      	ST   X,R30
         ;   10514                        NextCode++;
0046e2 940e 4b58 	CALL SUBOPT_0x80
         ;   10515                }
         ;   10516        }else
          _0xA09:
          _0xA08:
          _0xA06:
0046e4 c013      	RJMP _0xA0A
          _0xA04:
         ;   10517        if(NextCode == 15){
0046e5 81a8      	LD   R26,Y
0046e6 30af      	CPI  R26,LOW(0xF)
0046e7 f481      	BRNE _0xA0B
         ;   10518                if((Button == 1)||(Button == 2)){
0046e8 81a9      	LDD  R26,Y+1
0046e9 30a1      	CPI  R26,LOW(0x1)
0046ea f011      	BREQ _0xA0D
0046eb 30a2      	CPI  R26,LOW(0x2)
0046ec f459      	BRNE _0xA0C
          _0xA0D:
         ;   10519                        Data[NextCode] = Button + 0x30;   
0046ed 940e 4fa9 	CALL SUBOPT_0x12A
0046ef 93ff      	PUSH R31
0046f0 93ef      	PUSH R30
0046f1 81e9      	LDD  R30,Y+1
0046f2 5de0      	SUBI R30,-LOW(48)
0046f3 91af      	POP  R26
0046f4 91bf      	POP  R27
0046f5 93ec      	ST   X,R30
         ;   10520                        NextCode++;
0046f6 940e 4b58 	CALL SUBOPT_0x80
         ;   10521                }                       
         ;   10522        }      
          _0xA0C:
         ;   10523        if((NextCode == 7)||(NextCode == 10)||(NextCode == 13)) 
          _0xA0B:
          _0xA0A:
          _0xA03:
          _0xA00:
          _0x9FD:
          _0x9F6:
          _0x9F3:
0046f8 81a8      	LD   R26,Y
0046f9 30a7      	CPI  R26,LOW(0x7)
0046fa f021      	BREQ _0xA10
0046fb 30aa      	CPI  R26,LOW(0xA)
0046fc f011      	BREQ _0xA10
0046fd 30ad      	CPI  R26,LOW(0xD)
0046fe f411      	BRNE _0xA0F
          _0xA10:
         ;   10524                        NextCode++;  
0046ff 940e 4b58 	CALL SUBOPT_0x80
         ;   10525        if(NextCode >= 16)
          _0xA0F:
004701 81a8      	LD   R26,Y
004702 31a0      	CPI  R26,LOW(0x10)
004703 f010      	BRLO _0xA12
         ;   10526              NextCode = 0;  
004704 27ee      	CLR  R30
004705 83e8      	ST   Y,R30
         ;   10527        return(NextCode + 1);  
          _0xA12:
004706 81e8      	LD   R30,Y
004707 5fef      	SUBI R30,-LOW(1)
          _0xAAB:
004708 9622      	ADIW R28,2
004709 9508      	RET
         ;   10528        
         ;   10529 }  
         ;   10530  
         ;   10531 void Write_Timer(char num)
         ;   10532 {                   
          _Write_Timer:
         ;   10533      char i;
         ;   10534      char Day, Hour, Min, Arm; 
         ;   10535      num--;        
00470a 940e 4f99 	CALL SUBOPT_0x127
         ;	num -> Y+5
         ;	i -> R16
         ;	Day -> R17
         ;	Hour -> R18
         ;	Min -> R19
         ;	Arm -> R20
         ;   10536      Day = 0;
00470c e010      	LDI  R17,LOW(0)
         ;   10537      Pointer = Timer;
00470d efe0      	LDI  R30,LOW(496)
00470e e0f1      	LDI  R31,HIGH(496)
00470f 93e0 0691 	STS  _Pointer,R30
004711 93f0 0692 	STS  _Pointer+1,R31
         ;   10538      MaxNum = 4;
004713 e0e4      	LDI  R30,LOW(4)
004714 93e0 05eb 	STS  _MaxNum,R30
         ;   10539      for(i = 0;i < 7;i++)
004716 e000      	LDI  R16,LOW(0)
          _0xA15:
004717 3007      	CPI  R16,7
004718 f448      	BRSH _0xA16
         ;   10540         if(Data[i] != '*')
004719 940e 4af2 	CALL SUBOPT_0x71
00471b 32ea      	CPI  R30,LOW(0x2A)
00471c f019      	BREQ _0xA17
         ;   10541              Day|= (0x01<<i);  
00471d 940e 4e95 	CALL SUBOPT_0xFC
00471f 2b1e      	OR   R17,R30
         ;   10542      Hour = (Data[8]-0x30)*10 + (Data[9]-0x30);  
          _0xA17:
004720 5f0f      	SUBI R16,-1
004721 cff5      	RJMP _0xA15
          _0xA16:
004722   +  	__GETB1MN _Data,8
004722 91e0 05f5 	LDS  R30,_data+0x8
          	.ENDM
004724 940e 4ac8 	CALL SUBOPT_0x6B
004726 93ef      	PUSH R30
004727   +  	__GETB1MN _Data,9
004727 91e0 05f6 	LDS  R30,_data+0x9
          	.ENDM
004729 53e0      	SUBI R30,LOW(48)
00472a 91af      	POP  R26
00472b 0fea      	ADD  R30,R26
00472c 2f2e      	MOV  R18,R30
         ;   10543      Min =  (Data[11]-0x30)*10 + (Data[12]-0x30); 
00472d   +  	__GETB1MN _Data,11
00472d 91e0 05f8 	LDS  R30,_data+0xb
          	.ENDM
00472f 940e 4ac8 	CALL SUBOPT_0x6B
004731 93ef      	PUSH R30
004732   +  	__GETB1MN _Data,12
004732 91e0 05f9 	LDS  R30,_data+0xc
          	.ENDM
004734 53e0      	SUBI R30,LOW(48)
004735 91af      	POP  R26
004736 0fea      	ADD  R30,R26
004737 2f3e      	MOV  R19,R30
         ;   10544      if(Data[14] == 'S'){
004738   +  	__GETB1MN _Data,14
004738 91e0 05fb 	LDS  R30,_data+0xe
          	.ENDM
00473a 35e3      	CPI  R30,LOW(0x53)
00473b f459      	BRNE _0xA18
         ;   10545            Arm = Data[15]-0x30;    
00473c   +  	__GETB1MN _Data,15
00473c 91e0 05fc 	LDS  R30,_data+0xf
          	.ENDM
00473e 53e0      	SUBI R30,LOW(48)
00473f 2f4e      	MOV  R20,R30
         ;   10546            if((Arm != 1)&&(Arm != 2)) 
004740 3041      	CPI  R20,1
004741 f011      	BREQ _0xA1A
004742 3042      	CPI  R20,2
004743 f409      	BRNE _0xA1B
          _0xA1A:
004744 c001      	RJMP _0xA19
          _0xA1B:
         ;   10547                 Arm = 0;      
004745 e040      	LDI  R20,LOW(0)
         ;   10548      }else
          _0xA19:
004746 c017      	RJMP _0xA1C
          _0xA18:
         ;   10549      if(Data[14] == 'A'){   
004747   +  	__GETB1MN _Data,14
004747 91e0 05fb 	LDS  R30,_data+0xe
          	.ENDM
004749 34e1      	CPI  R30,LOW(0x41)
00474a f461      	BRNE _0xA1D
         ;   10550            Arm = Data[15]-0x30; 
00474b   +  	__GETB1MN _Data,15
00474b 91e0 05fc 	LDS  R30,_data+0xf
          	.ENDM
00474d 53e0      	SUBI R30,LOW(48)
00474e 2f4e      	MOV  R20,R30
         ;   10551            if((Arm == 1)||(Arm == 2)) 
00474f 3041      	CPI  R20,1
004750 f011      	BREQ _0xA1F
004751 3042      	CPI  R20,2
004752 f411      	BRNE _0xA1E
          _0xA1F:
         ;   10552                 Arm+= 2;
004753 5f4e      	SUBI R20,-LOW(2)
         ;   10553            else
004754 c001      	RJMP _0xA21
          _0xA1E:
         ;   10554                 Arm = 0;     
004755 e040      	LDI  R20,LOW(0)
          _0xA21:
         ;   10555      }else          
004756 c007      	RJMP _0xA22
          _0xA1D:
         ;   10556      if(Data[14] == 'D'){  
004757   +  	__GETB1MN _Data,14
004757 91e0 05fb 	LDS  R30,_data+0xe
          	.ENDM
004759 34e4      	CPI  R30,LOW(0x44)
00475a f411      	BRNE _0xA23
         ;   10557            Arm = 5;          
00475b e045      	LDI  R20,LOW(5)
         ;   10558      }else
00475c c001      	RJMP _0xA24
          _0xA23:
         ;   10559            Arm = 0;       
00475d e040      	LDI  R20,LOW(0)
          _0xA24:
          _0xA22:
          _0xA1C:
         ;   10560      Data[0] = StatusTimer[0][num] = Day;
00475e 81ad      	LDD  R26,Y+5
00475f 27bb      	CLR  R27
004760 50af      	SUBI R26,LOW(-_StatusTimer)
004761 4fb8      	SBCI R27,HIGH(-_StatusTimer)
004762 2fe1      	MOV  R30,R17
004763 93ec      	ST   X,R30
004764 93e0 05ed 	STS  _Data,R30
         ;   10561      Data[1] = StatusTimer[1][num] = Hour;
004766   +  	__POINTW2MN _StatusTimer,10
004766 efab      	LDI  R26,LOW(_statustimer+0xa)
004767 e0b7      	LDI  R27,HIGH(_statustimer+0xa)
          	.ENDM
004768 81ed      	LDD  R30,Y+5
004769 27ff      	CLR  R31
00476a 0fae      	ADD  R26,R30
00476b 1fbf      	ADC  R27,R31
00476c 2fe2      	MOV  R30,R18
00476d 93ec      	ST   X,R30
00476e   +  	__PUTB1MN _Data,1
00476e 93e0 05ee 	STS  _data+0x1,R30
          	.ENDM
         ;   10562      Data[2] = StatusTimer[2][num] = Min;
004770   +  	__POINTW2MN _StatusTimer,20
004770 e0a5      	LDI  R26,LOW(_statustimer+0x14)
004771 e0b8      	LDI  R27,HIGH(_statustimer+0x14)
          	.ENDM
004772 81ed      	LDD  R30,Y+5
004773 27ff      	CLR  R31
004774 0fae      	ADD  R26,R30
004775 1fbf      	ADC  R27,R31
004776 2fe3      	MOV  R30,R19
004777 93ec      	ST   X,R30
004778   +  	__PUTB1MN _Data,2
004778 93e0 05ef 	STS  _data+0x2,R30
          	.ENDM
         ;   10563      Data[3] = StatusTimer[3][num] = Arm;
00477a   +  	__POINTW2MN _StatusTimer,30
00477a e0af      	LDI  R26,LOW(_statustimer+0x1e)
00477b e0b8      	LDI  R27,HIGH(_statustimer+0x1e)
          	.ENDM
00477c 81ed      	LDD  R30,Y+5
00477d 27ff      	CLR  R31
00477e 0fae      	ADD  R26,R30
00477f 1fbf      	ADC  R27,R31
004780 2fe4      	MOV  R30,R20
004781 93ec      	ST   X,R30
004782   +  	__PUTB1MN _Data,3
004782 93e0 05f0 	STS  _data+0x3,R30
          	.ENDM
         ;   10564      WriteData(num+1);  
004784 81ed      	LDD  R30,Y+5
004785 5fef      	SUBI R30,-LOW(1)
004786 93ea      	ST   -Y,R30
004787 940e 23ee 	CALL _WriteData
         ;   10565 } 
          _0xAAA:
004789 940e 50a7 	CALL __LOADLOCR5
00478b 9626      	ADIW R28,6
00478c 9508      	RET
         ;   10566 void Read_Timer(void)
         ;   10567 {      
         ;   10568      char i; 
         ;   10569      Pointer = Timer;
         ;	i -> R16
         ;   10570      MaxNum = 4;  
         ;   10571      for(i = 0;i<MaxTimer;i++){
         ;   10572          WriteData(i+1);        
         ;   10573          StatusTimer[0][i] = Data[0];
         ;   10574          StatusTimer[1][i] = Data[1];
         ;   10575          StatusTimer[2][i] = Data[2];
         ;   10576          StatusTimer[3][i] = Data[3];     
         ;   10577      }    
         ;   10578 } 
         ;   10579 char TestTimer(char Week,char Hour, char Min)
         ;   10580 {     
          _TestTimer:
         ;   10581      char i;
         ;   10582      char pWeek, pHour, pMin, pArm = 0; 
         ;   10583      Week = 0x01<<(Week - 1);             
00478d 940e 50a0 	CALL __SAVELOCR5
         ;	Week -> Y+7
         ;	Hour -> Y+6
         ;	Min -> Y+5
         ;	i -> R16
         ;	pWeek -> R17
         ;	pHour -> R18
         ;	pMin -> R19
         ;	pArm -> R20
00478f e040      	LDI  R20,0
004790 81ef      	LDD  R30,Y+7
004791 50e1      	SUBI R30,LOW(1)
004792 e0a1      	LDI  R26,LOW(1)
004793 940e 503c 	CALL __LSLB12
004795 83ef      	STD  Y+7,R30
         ;   10584           
         ;   10585      for(i = 0; (i<MaxTimer)&&(!pArm); i++){
004796 e000      	LDI  R16,LOW(0)
          _0xA2B:
004797 300a      	CPI  R16,10
004798 f410      	BRSH _0xA2D
004799 3040      	CPI  R20,0
00479a f009      	BREQ _0xA2E
          _0xA2D:
00479b c01e      	RJMP _0xA2C
          _0xA2E:
         ;   10586            pWeek = StatusTimer[0][i];
00479c 2fe0      	MOV  R30,R16
00479d 940e 4f9f 	CALL SUBOPT_0x128
         ;   10587            pHour = StatusTimer[1][i];
00479f   +  	__POINTW2MN _StatusTimer,10
00479f efab      	LDI  R26,LOW(_statustimer+0xa)
0047a0 e0b7      	LDI  R27,HIGH(_statustimer+0xa)
          	.ENDM
0047a1 940e 4fae 	CALL SUBOPT_0x12B
0047a3 912c      	LD   R18,X
         ;   10588            pMin  = StatusTimer[2][i];
0047a4   +  	__POINTW2MN _StatusTimer,20
0047a4 e0a5      	LDI  R26,LOW(_statustimer+0x14)
0047a5 e0b8      	LDI  R27,HIGH(_statustimer+0x14)
          	.ENDM
0047a6 940e 4fae 	CALL SUBOPT_0x12B
0047a8 913c      	LD   R19,X
         ;   10589            if((pWeek & Week)&&(pHour == Hour)&&(pMin == Min))
0047a9 81ef      	LDD  R30,Y+7
0047aa 23e1      	AND  R30,R17
0047ab f031      	BREQ _0xA30
0047ac 81ee      	LDD  R30,Y+6
0047ad 17e2      	CP   R30,R18
0047ae f419      	BRNE _0xA30
0047af 81ed      	LDD  R30,Y+5
0047b0 17e3      	CP   R30,R19
0047b1 f009      	BREQ _0xA31
          _0xA30:
0047b2 c005      	RJMP _0xA2F
          _0xA31:
         ;   10590                       pArm  = StatusTimer[3][i];
0047b3   +  	__POINTW2MN _StatusTimer,30
0047b3 e0af      	LDI  R26,LOW(_statustimer+0x1e)
0047b4 e0b8      	LDI  R27,HIGH(_statustimer+0x1e)
          	.ENDM
0047b5 940e 4fae 	CALL SUBOPT_0x12B
0047b7 914c      	LD   R20,X
         ;   10591      }   
          _0xA2F:
0047b8 5f0f      	SUBI R16,-1
0047b9 cfdd      	RJMP _0xA2B
          _0xA2C:
         ;   10592      return(pArm);              
0047ba 2fe4      	MOV  R30,R20
0047bb 940e 50a7 	CALL __LOADLOCR5
0047bd 9628      	ADIW R28,8
0047be 9508      	RET
         ;   10593 }
         ;   10594 #include <mega128.h> 
         ;   10595 // CodeVisionAVR C Compiler
         ;   10596 // (C) 1998-2001 Pavel Haiduc, HP InfoTech S.R.L.
         ;   10597 
         ;   10598 // I/O registers definitions for the ATmega128
         ;   10599 
         ;   10600 #ifndef _MEGA128_INCLUDED_
         ;   10601 #define _MEGA128_INCLUDED_
         ;   10602 
         ;   10603 #pragma used+
         ;   10604 sfrb PINF=0;
         ;   10605 sfrb PINE=1;
         ;   10606 sfrb DDRE=2;
         ;   10607 sfrb PORTE=3;
         ;   10608 sfrb ADCL=4;
         ;   10609 sfrb ADCH=5;
         ;   10610 sfrw ADCW=4;      // 16 bit access
         ;   10611 sfrb ADCSRA=6;
         ;   10612 sfrb ADMUX=7;
         ;   10613 sfrb ACSR=8;
         ;   10614 sfrb UBRR0L=9;
         ;   10615 sfrb UCSR0B=0xa;
         ;   10616 sfrb UCSR0A=0xb;
         ;   10617 sfrb UDR0=0xc;
         ;   10618 sfrb SPCR=0xd;
         ;   10619 sfrb SPSR=0xe;
         ;   10620 sfrb SPDR=0xf;
         ;   10621 sfrb PIND=0x10;
         ;   10622 sfrb DDRD=0x11;
         ;   10623 sfrb PORTD=0x12;
         ;   10624 sfrb PINC=0x13;
         ;   10625 sfrb DDRC=0x14;
         ;   10626 sfrb PORTC=0x15;
         ;   10627 sfrb PINB=0x16;
         ;   10628 sfrb DDRB=0x17;
         ;   10629 sfrb PORTB=0x18;
         ;   10630 sfrb PINA=0x19;
         ;   10631 sfrb DDRA=0x1a;
         ;   10632 sfrb PORTA=0x1b;
         ;   10633 sfrb EECR=0x1c;
         ;   10634 sfrb EEDR=0x1d;
         ;   10635 sfrb EEARL=0x1e;
         ;   10636 sfrb EEARH=0x1f;
         ;   10637 sfrw EEAR=0x1e;   // 16 bit access
         ;   10638 sfrb SFIOR=0x20;
         ;   10639 sfrb WDTCR=0x21;
