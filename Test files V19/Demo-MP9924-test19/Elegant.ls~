         ;    5293 #define        pEraseUnitR     (pEraseUnit * Max) 
         ;    5294 #define        pEraseAllR      (pEraseAll * Max) 
         ;    5295 
         ;    5296 
         ;    5297 // USART1 Receiver buffer
         ;    5298 char rx_buffer1[RX_BUFFER_SIZE1];
         
          	.DSEG
          _rx_buffer1:
000693      	.BYTE 0x40
         ;    5299 unsigned char rx_wr_index1, rx_rd_index1;
          _rx_wr_index1:
0006d3      	.BYTE 0x1
          _rx_rd_index1:
0006d4      	.BYTE 0x1
         ;    5300               
         ;    5301 // USART1 Transmitter buffer
         ;    5302 char tx_buffer1[TX_BUFFER_SIZE1];                   
          _tx_buffer1:
0006d5      	.BYTE 0x40
         ;    5303 
         ;    5304 unsigned char tx_rd_index1;
          _tx_rd_index1:
000715      	.BYTE 0x1
         ;    5305 unsigned char StatusUart1;   // 100 Ms  
          _StatusUart1:
000716      	.BYTE 0x1
         ;    5306 unsigned char  mConected, tConected;
          _mConected:
000717      	.BYTE 0x1
          _tConected:
000718      	.BYTE 0x1
         ;    5307 unsigned char  mCanSend, tCanSend, CanEnter;
          _mCanSend:
000719      	.BYTE 0x1
          _tCanSend:
00071a      	.BYTE 0x1
          _CanEnter:
00071b      	.BYTE 0x1
         ;    5308 unsigned char TimeOut1, TimeConect;
          _TimeOut1:
00071c      	.BYTE 0x1
          _TimeConect:
00071d      	.BYTE 0x1
         ;    5309 unsigned char CountTel, CountMoked;   // number of telephon
          _CountTel:
