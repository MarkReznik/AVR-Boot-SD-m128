003532 0000      	 nop 
003533 5f0f      	SUBI R16,-1
003534 cffb      	RJMP _0x79C
          _0x79D:
         ;    7237 }
003535 9109      	LD   R16,Y+
003536 9508      	RET
         ;    7238 void Delay2_5Ms(void)    // delay of 2.5 Ms
         ;    7239 {
          _Delay2_5Ms:
         ;    7240      int i;
         ;    7241      for(i = 0; i < 1200;i++)
003537 931a      	ST   -Y,R17
003538 930a      	ST   -Y,R16
         ;	i -> R16,R17
003539   +  	__GETWRN 16,17,0
003539 e000      	LDI  R0x10,LOW(0x0)
00353a e010      	LDI  R0x11,HIGH(0x0)
          	.ENDM
          _0x7A0:
00353b   +  	__CPWRN 16,17,1200
00353b 3b00      	CPI  R0x10,LOW(0x4b0)
00353c e0e4      	LDI  R30,HIGH(0x4b0)
00353d 071e      	CPC  R0x11,R30
          	.ENDM
00353e f424      	BRGE _0x7A1
         ;    7242        #asm(" nop ");  
00353f 0000      	 nop 
003540   +  	__ADDWRN 16,17,1
003540 5f0f      	SUBI R0x10,LOW(-0x1)
003541 4f1f      	SBCI R0x11,HIGH(-0x1)
          	.ENDM
003542 cff8      	RJMP _0x7A0
          _0x7A1:
         ;    7243 }
003543 9109      	LD   R16,Y+
003544 9119      	LD   R17,Y+
003545 9508      	RET
