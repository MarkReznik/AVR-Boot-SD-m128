         ;    9170 void CaseEvent(char Type,char NumCode)
         ;    9171 {    
         
          	.CSEG
          _CaseEvent:
         ;    9172     int MaxEvent;
         ;    9173     Pointer = DisplayEvent;
004048 931a      	ST   -Y,R17
004049 930a      	ST   -Y,R16
         ;	Type -> Y+3
         ;	NumCode -> Y+2
         ;	MaxEvent -> R16,R17
00404a eaee      	LDI  R30,LOW(942)
00404b e0f3      	LDI  R31,HIGH(942)
00404c 940e 4966 	CALL SUBOPT_0x3D
         ;    9174     MaxNum = 16;        
         ;    9175     MaxEvent = ListEvent;
00404e 91e0 07d4 	LDS  R30,_ListEvent
004050 27ff      	CLR  R31
004051   +  	__PUTW1R 16,17
004051 2f0e      	MOV  R0x10,R30
004052 2f1f      	MOV  R0x11,R31
          	.ENDM
         ;    9176     MaxEvent*= MaxNum;
004053 91e0 05eb 	LDS  R30,_MaxNum
004055   +  	__GETW2R 16,17
004055 2fa0      	MOV  R26,R0x10
004056 2fb1      	MOV  R27,R0x11
          	.ENDM
004057 27ff      	CLR  R31
004058 940e 5052 	CALL __MULW12U
00405a   +  	__PUTW1R 16,17
00405a 2f0e      	MOV  R0x10,R30
00405b 2f1f      	MOV  R0x11,R31
          	.ENDM
         ;    9177     MaxEvent+= DisplayEvent;
00405c   +  	__ADDWRN 16,17,942
00405c 5502      	SUBI R0x10,LOW(-0x3ae)
00405d 4f1c      	SBCI R0x11,HIGH(-0x3ae)
          	.ENDM
         ;    9178     if(MaxEvent < 4000)
00405e   +  	__CPWRN 16,17,4000
00405e 3a00      	CPI  R0x10,LOW(0xfa0)
00405f e0ef      	LDI  R30,HIGH(0xfa0)
004060 071e      	CPC  R0x11,R30
          	.ENDM
004061 f42c      	BRGE _0x921
         ;    9179            ListEvent++;   
004062 91e0 07d4 	LDS  R30,_ListEvent
004064 5fef      	SUBI R30,-LOW(1)
004065 93e0 07d4 	STS  _ListEvent,R30
         ;    9180         
         ;    9181     Data[0] = Type;
          _0x921:
004067 81eb      	LDD  R30,Y+3
004068 93e0 05ed 	STS  _Data,R30
         ;    9182     Data[1] = NumCode;     
00406a 81ea      	LDD  R30,Y+2
00406b   +  	__PUTB1MN _Data,1
00406b 93e0 05ee 	STS  _data+0x1,R30
          	.ENDM
         ;    9183     Data[2] = TimeHour;
00406d   +  	__POINTW2MN _Data,2
00406d eeaf      	LDI  R26,LOW(_data+0x2)
00406e e0b5      	LDI  R27,HIGH(_data+0x2)
          	.ENDM
00406f 91e0 07da 	LDS  R30,_TimeHour
004071 93ec      	ST   X,R30
         ;    9184     Data[3] = TimeMin;       
004072   +  	__POINTW2MN _Data,3
004072 efa0      	LDI  R26,LOW(_data+0x3)
004073 e0b5      	LDI  R27,HIGH(_data+0x3)
          	.ENDM
004074 91e0 07db 	LDS  R30,_TimeMin
004076 93ec      	ST   X,R30
         ;    9185     Data[4] = Day;
004077   +  	__POINTW2MN _Data,4
004077 efa1      	LDI  R26,LOW(_data+0x4)
004078 e0b5      	LDI  R27,HIGH(_data+0x4)
          	.ENDM
004079 91e0 07e2 	LDS  R30,_Day
00407b 93ec      	ST   X,R30
         ;    9186     Data[5] = Month;     
00407c   +  	__POINTW2MN _Data,5
00407c efa2      	LDI  R26,LOW(_data+0x5)
00407d e0b5      	LDI  R27,HIGH(_data+0x5)
          	.ENDM
00407e 91e0 07e1 	LDS  R30,_Month
004080 93ec      	ST   X,R30
         ;    9187     Data[6] = Year;    
004081   +  	__POINTW2MN _Data,6
004081 efa3      	LDI  R26,LOW(_data+0x6)
004082 e0b5      	LDI  R27,HIGH(_data+0x6)
          	.ENDM
