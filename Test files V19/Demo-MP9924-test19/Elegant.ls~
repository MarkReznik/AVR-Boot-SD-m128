001f8e 93e0 072c 	STS  _pStopEvent,R30
         ;    3984      CallSirenUp = 0; 
          _0x497:
001f90 94e8      	CLT
001f91 f824      	BLD  R2,4
         ;    3985      Siren_Stop();
001f92 d029      	RCALL _Siren_Stop
         ;    3986      Panic = 0;   // Close Status Panic
001f93 94e8      	CLT
001f94 f831      	BLD  R3,1
         ;    3987      ValueSiren = 0;
001f95 27ee      	CLR  R30
001f96 93e0 0647 	STS  _ValueSiren,R30
         ;    3988      LightByte = LightByte &~0x01;    // turn off light 
001f98 940e 48b8 	CALL SUBOPT_0x24
         ;    3989      RelayOff(R_LIGHT);
         ;    3990      ClearBit(Zone,0x08);             // clear bit of display
001f9a eaef      	LDI  R30,LOW(_Zone)
001f9b e0f7      	LDI  R31,HIGH(_Zone)
001f9c 93fa      	ST   -Y,R31
001f9d 93ea      	ST   -Y,R30
001f9e e0e8      	LDI  R30,LOW(8)
001f9f 93ea      	ST   -Y,R30
001fa0 940e 248b 	CALL _ClearBit
         ;    3991      DisplayZone = 0x80;                   // display 
001fa2 e8e0      	LDI  R30,LOW(128)
001fa3 93e0 065e 	STS  _DisplayZone,R30
         ;    3992      if(ButtonChime & 0x03)  
001fa5 91e0 065d 	LDS  R30,_ButtonChime
001fa7 70e3      	ANDI R30,LOW(0x3)
001fa8 f029      	BREQ _0x498
         ;    3993                DisplayLed(0x10,2); 
001fa9 e1e0      	LDI  R30,LOW(16)
001faa 940e 4897 	CALL SUBOPT_0x1E
001fac 940e 455a 	CALL _DisplayLed
         ;    3994      if(ButtonChime & 0x0C)
          _0x498:
001fae 91e0 065d 	LDS  R30,_ButtonChime
001fb0 70ec      	ANDI R30,LOW(0xC)
001fb1 f029      	BREQ _0x499
         ;    3995                DisplayLed(0x20,2);     
001fb2 e2e0      	LDI  R30,LOW(32)
001fb3 940e 4897 	CALL SUBOPT_0x1E
001fb5 940e 455a 	CALL _DisplayLed
         ;    3996 }             
          _0x499:
001fb7 9508      	RET
         ;    3997 void Siren_Start(void)
         ;    3998 {               
          _Siren_Start:
         ;    3999     TCCR3B=0x1A;
