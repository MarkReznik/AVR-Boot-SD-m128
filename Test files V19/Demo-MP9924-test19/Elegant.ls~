          _0x8F1:
         ;    8782                  } // else
          _0x8F0:
          _0x8EF:
          _0x8E5:
          _0x8E0:
         ;    8783         }  // if((Zone[ind] & 0x80)&&(Level)) 
         ;    8784         ind++;     
          _0x8DC:
003fb6 5f0f      	SUBI R16,-1
         ;    8785      } // while
003fb7 cf7c      	RJMP _0x8D9
          _0x8DB:
         ;    8786      if((Find)&&(!OpenZoneKey))           
003fb8 3010      	CPI  R17,0
003fb9 f011      	BREQ _0x8F3
003fba fe30      	SBRS R3,0
003fbb c001      	RJMP _0x8F4
          _0x8F3:
003fbc c014      	RJMP _0x8F2
          _0x8F4:
         ;    8787      {            
         ;    8788            OpenZoneKey = 1; 
003fbd 9468      	SET
003fbe f830      	BLD  R3,0
         ;    8789            if((!StatusArm)&&(Status < 4))
003fbf 91e0 0658 	LDS  R30,_StatusArm
003fc1 30e0      	CPI  R30,0
003fc2 f421      	BRNE _0x8F6
003fc3 91a0 05d9 	LDS  R26,_Status
003fc5 30a4      	CPI  R26,LOW(0x4)
003fc6 f008      	BRLO _0x8F7
          _0x8F6:
003fc7 c002      	RJMP _0x8F5
          _0x8F7:
         ;    8790                 StartToArm();
003fc8 940e 1388 	CALL _StartToArm
         ;    8791            if(!Status)     
          _0x8F5:
003fca 91e0 05d9 	LDS  R30,_Status
003fcc 30e0      	CPI  R30,0
003fcd f419      	BRNE _0x8F8
         ;    8792                 Key_SA(Find);                                                                 
003fce 931a      	ST   -Y,R17
003fcf 940e 0f9e 	CALL _Key_SA
         ;    8793      } 
          _0x8F8:
         ;    8794      if(!Find)
          _0x8F2:
003fd1 3010      	CPI  R17,0
003fd2 f411      	BRNE _0x8F9
         ;    8795           OpenZoneKey = 0;         
003fd3 94e8      	CLT
003fd4 f830      	BLD  R3,0
         ;    8796      return(bZoneType);
          _0x8F9:
003fd5 91e0 07cf 	LDS  R30,_bZoneType
003fd7 940e 50a8 	CALL __LOADLOCR4
003fd9 9624      	ADIW R28,4
003fda 9508      	RET
         ;    8797      
         ;    8798 }
         ;    8799 void ListZones(int StutLine1)   //  to list names of zones
         ;    8800 {   
          _ListZones:
         ;    8801    unsigned char Count = 0;
         ;    8802    unsigned char BitHelp;
         ;    8803    unsigned char Find = 0;
         ;    8804  
         ;    8805    if(StutLine1 == pOpenZoneR) BitHelp = DisplayZone;  
003fdb 940e 50a2 	CALL __SAVELOCR3
         ;	StutLine1 -> Y+3
         ;	Count -> R16
         ;	BitHelp -> R17
         ;	Find -> R18
003fdd e000      	LDI  R16,0
003fde e020      	LDI  R18,0
003fdf 81ab      	LDD  R26,Y+3
003fe0 81bc      	LDD  R27,Y+3+1
003fe1 35aa      	CPI  R26,LOW(0x5A)
003fe2 e0e0      	LDI  R30,HIGH(0x5A)
003fe3 07be      	CPC  R27,R30
003fe4 f411      	BRNE _0x8FB
003fe5 9110 065e 	LDS  R17,_DisplayZone
         ;    8806    if(StutLine1 == pBypassZoneR) BitHelp = 0x40;
          _0x8FB:
003fe7 81ab      	LDD  R26,Y+3
003fe8 81bc      	LDD  R27,Y+3+1
003fe9 36a3      	CPI  R26,LOW(0x63)
003fea e0e0      	LDI  R30,HIGH(0x63)
003feb 07be      	CPC  R27,R30
003fec f409      	BRNE _0x8FC
003fed e410      	LDI  R17,LOW(64)
         ;    8807    if(StutLine1 == pActivZoneR ) BitHelp = 0x10;
          _0x8FC:
003fee 81ab      	LDD  R26,Y+3
003fef 81bc      	LDD  R27,Y+3+1
003ff0 36ac      	CPI  R26,LOW(0x6C)
003ff1 e0e0      	LDI  R30,HIGH(0x6C)
003ff2 07be      	CPC  R27,R30
003ff3 f409      	BRNE _0x8FD
003ff4 e110      	LDI  R17,LOW(16)
         ;    8808    
         ;    8809    while((!Find)&&(Count<=17))
          _0x8FD:
          _0x8FE:
003ff5 3020      	CPI  R18,0
003ff6 f419      	BRNE _0x901
003ff7 e1e1      	LDI  R30,LOW(17)
003ff8 17e0      	CP   R30,R16
003ff9 f408      	BRSH _0x902
          _0x901:
003ffa c036      	RJMP _0x900
          _0x902:
         ;    8810    {
         ;    8811       if(List >= 16)
003ffb 91a0 05e6 	LDS  R26,_List
003ffd 31a0      	CPI  R26,LOW(0x10)
003ffe f028      	BRLO _0x903
         ;    8812       {
         ;    8813            List = 0;
003fff 27ee      	CLR  R30
004000 93e0 05e6 	STS  _List,R30
         ;    8814            CycleListZone = 0x01;    // second sycle of list zone
004002 9468      	SET
004003 f827      	BLD  R2,7
         ;    8815       }     
         ;    8816       if((Zone[List]&0x08)&&(!Status)){
          _0x903:
004004 940e 4f3d 	CALL SUBOPT_0x116
004006 70e8      	ANDI R30,LOW(0x8)
004007 f021      	BREQ _0x905
004008 91e0 05d9 	LDS  R30,_Status
00400a 30e0      	CPI  R30,0
00400b f009      	BREQ _0x906
          _0x905:
00400c c004      	RJMP _0x904
          _0x906:
         ;    8817             BitHelp = DisplayZone = 0x08;        
00400d e0e8      	LDI  R30,LOW(8)
00400e 93e0 065e 	STS  _DisplayZone,R30
004010 2f1e      	MOV  R17,R30
         ;    8818       }      
         ;    8819       if((Zone[List]&BitHelp)||(StutLine1 == pListZoneR))
          _0x904:
004011 940e 4f3d 	CALL SUBOPT_0x116
004013 23e1      	AND  R30,R17
004014 f431      	BRNE _0x908
004015 81ab      	LDD  R26,Y+3
004016 81bc      	LDD  R27,Y+3+1
004017 37a5      	CPI  R26,LOW(0x75)
004018 e0e0      	LDI  R30,HIGH(0x75)
004019 07be      	CPC  R27,R30
00401a f491      	BRNE _0x907
          _0x908:
         ;    8820          if(((DataType[List]&0x0F)&&(DataType[List] != 0x35))||(Status == 2))
00401b 91e0 05e6 	LDS  R30,_List
00401d 940e 4eea 	CALL SUBOPT_0x109
00401f f041      	BREQ _0x90B
004020 91e0 05e6 	LDS  R30,_List
004022 27ff      	CLR  R31
004023 50e2      	SUBI R30,LOW(-_DataType)
004024 4ffa      	SBCI R31,HIGH(-_DataType)
004025 81e0      	LD   R30,Z
004026 33e5      	CPI  R30,LOW(0x35)
004027 f421      	BRNE _0x90D
          _0x90B:
004028 91a0 05d9 	LDS  R26,_Status
00402a 30a2      	CPI  R26,LOW(0x2)
00402b f409      	BRNE _0x90A
          _0x90D:
         ;    8821          { 
         ;    8822              Find = 1;                                                                                      
00402c e021      	LDI  R18,LOW(1)
         ;    8823          }  
         ;    8824       Count++;
          _0x90A:
          _0x907:
00402d 5f0f      	SUBI R16,-1
         ;    8825       List++;
00402e 940e 4a9b 	CALL SUBOPT_0x63
         ;    8826        
         ;    8827    }
004030 cfc4      	RJMP _0x8FE
          _0x900:
         ;    8828    if(!Find)
004031 3020      	CPI  R18,0
004032 f489      	BRNE _0x90F
         ;    8829    {
         ;    8830        List = 0;  
004033 27ee      	CLR  R30
004034 93e0 05e6 	STS  _List,R30
         ;    8831        if(!StatusArm)                  // if zone DisplayZone was open
004036 91e0 0658 	LDS  R30,_StatusArm
004038 30e0      	CPI  R30,0
004039 f419      	BRNE _0x910
         ;    8832             ValueExit = 0;
00403a 27ee      	CLR  R30
00403b 93e0 0653 	STS  _ValueExit,R30
         ;    8833        if(WasSiren)
          _0x910:
00403d fe25      	SBRS R2,5
00403e c002      	RJMP _0x911
         ;    8834            DisplayZone = 0x10;
00403f e1e0      	LDI  R30,LOW(16)
004040 c001      	RJMP _0xB01
         ;    8835        else    
          _0x911:
         ;    8836            DisplayZone = 0x80; 
004041 e8e0      	LDI  R30,LOW(128)
          _0xB01:
004042 93e0 065e 	STS  _DisplayZone,R30
         ;    8837    }       
         ;    8838 }
          _0x90F:
004044 940e 50a9 	CALL __LOADLOCR3
004046 9625      	ADIW R28,5
004047 9508      	RET
