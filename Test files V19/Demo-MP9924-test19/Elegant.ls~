0027da f009      	BREQ _0x5CD
          _0x5CC:
0027db c002      	RJMP _0x5CB
          _0x5CD:
         ;    5843                return(1);   
0027dc e0e1      	LDI  R30,LOW(1)
0027dd 9508      	RET
         ;    5844     else
          _0x5CB:
         ;    5845                return(0);         
0027de 27ee      	CLR  R30
0027df 9508      	RET
         ;    5846 }
0027e0 9508      	RET
         ;    5847 
         ;    5848 void ButtonSpike(char Key)
         ;    5849 {
          _ButtonSpike:
         ;    5850           switch(Status)
0027e1 91e0 05d9 	LDS  R30,_Status
         ;    5851           {     
         ;    5852                 case 0:   // arm or disarm
0027e3 30e0      	CPI  R30,0
0027e4 f011      	BREQ PC+3
0027e5 940c 2885 	JMP _0x5D3
         ;    5853                         if((Key > 0)&&(Key <= 10)){   
0027e7 940e 4d47 	CALL SUBOPT_0xC2
0027e9 f418      	BRSH _0x5D5
0027ea 940e 4d4b 	CALL SUBOPT_0xC3
0027ec f408      	BRSH _0x5D6
          _0x5D5:
0027ed c00b      	RJMP _0x5D4
          _0x5D6:
         ;    5854                                StopMessage();                                       
0027ee 940e 2692 	CALL _StopMessage
         ;    5855                                if(!ButtonSACB){                                                         
