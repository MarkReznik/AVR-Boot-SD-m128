         ;    5508 void SpikeMessage(int Address,char Time)
         ;    5509 {              
          _SpikeMessage:
         ;    5510        // Port F
         ;    5511        PORTF=0x00;
00265c 940e 4ce1 	CALL SUBOPT_0xB4
         ;    5512        DDRF=0xFF;     // output     
         ;    5513        #asm("NOP"); 
00265e 0000      	NOP
         ;    5514        PORTC|= 0x40;        // disenable                  
00265f 9aae      	SBI  0x15,6
         ;    5515        #asm("NOP");                  
002660 0000      	NOP
         ;    5516        PORTC|= 0x20;        // P/R   
002661 9aad      	SBI  0x15,5
         ;    5517        #asm("NOP"); 
002662 0000      	NOP
         ;    5518        //PORTC|= 0x80;        // PD  
         ;    5519        PORTF = Address;        // Address   
002663 81e9      	LDD  R30,Y+1
002664 81fa      	LDD  R31,Y+1+1
002665 940e 4ce8 	CALL SUBOPT_0xB5
         ;    5520        PORTC&=~0x03;  
         ;    5521        PORTC|= (0x03 & (Address>>8));        // Address  
002667 93ef      	PUSH R30
002668 81e9      	LDD  R30,Y+1
002669 81fa      	LDD  R31,Y+1+1
00266a 940e 504d 	CALL __ASRw8
00266c 70e3      	ANDI R30,LOW(0x3)
00266d 70f0      	ANDI R31,HIGH(0x3)
00266e 91af      	POP  R26
00266f 940e 4cf9 	CALL SUBOPT_0xB6
         ;    5522        PauseSpike = 5;          
         ;    5523        while(PauseSpike);   
          _0x566:
002671 91e0 0784 	LDS  R30,_PauseSpike
002673 30e0      	CPI  R30,0
002674 f7e1      	BRNE _0x566
         ;    5524        PORTC&=~0x80;        // PD  
002675 940e 4d01 	CALL SUBOPT_0xB7
002677 940e 4d08 	CALL SUBOPT_0xB8
         ;    5525        #asm("NOP");                 
002679 0000      	NOP
         ;    5526        PORTC&=~0x20;        // P/R   
00267a b3e5      	IN   R30,0x15
00267b 01df      	MOVW R26,R30
00267c 01cb      	MOVW R24,R22
00267d 940e 497b 	CALL SUBOPT_0x41
00267f bbe5      	OUT  0x15,R30
         ;    5527        #asm("NOP"); 
002680 0000      	NOP
         ;    5528        PORTC&=~0x40;        // enable   
002681 940e 4d01 	CALL SUBOPT_0xB7
002683 940e 4d10 	CALL SUBOPT_0xB9
         ;    5529                      
         ;    5530        StatusSpike = 1;   
002685 e0e1      	LDI  R30,LOW(1)
002686 93e0 0781 	STS  _StatusSpike,R30
         ;    5531        CounterSecond = 0;
002688 27ee      	CLR  R30
002689 93e0 0673 	STS  _CounterSecond,R30
