          _MikrofonIn:
00077f      	.BYTE 0x1
         ;    5325 unsigned char PhoneUp;
          _PhoneUp:
000780      	.BYTE 0x1
         ;    5326 
         ;    5327 //unsigned char GetMessage, Message,SendMessage;
         ;    5328 unsigned char StatusSpike, TimeSpike, TelTone,PauseSpike,NextKey;     
          _StatusSpike:
000781      	.BYTE 0x1
          _TimeSpike:
000782      	.BYTE 0x1
          _TelTone:
