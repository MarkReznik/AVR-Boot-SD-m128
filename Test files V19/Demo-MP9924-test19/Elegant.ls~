          	.DSEG
          _RelaysOut:
00079e      	.BYTE 0x10
         ;    7177 unsigned char LongRalay;
          _LongRalay:
0007ae      	.BYTE 0x1
         ;    7178 
         ;    7179 void SendToRelay(char Relay)
         ;    7180 {
         
          	.CSEG
          _SendToRelay:
         ;    7181      int Data_Rely = 0;
         ;    7182      char ind,j;
         ;    7183      Data_Rely = Relay;		
0034b7 940e 50a1 	CALL __SAVELOCR4
         ;	Relay -> Y+4
         ;	Data_Rely -> R16,R17
         ;	ind -> R18
         ;	j -> R19
0034b9 e000      	LDI  R16,0
0034ba e010      	LDI  R17,0
0034bb 81ec      	LDD  R30,Y+4
0034bc 27ff      	CLR  R31
0034bd   +  	__PUTW1R 16,17
0034bd 2f0e      	MOV  R0x10,R30
0034be 2f1f      	MOV  R0x11,R31
          	.ENDM
         ;    7184      Data_Rely = (Data_Rely << 8)|0x1F;     
0034bf   +  	__GETW1R 16,17
0034bf 2fe0      	MOV  R30,R0x10
0034c0 2ff1      	MOV  R31,R0x11
          	.ENDM
0034c1 2ffe      	MOV  R31,R30
0034c2 27ee      	CLR  R30
0034c3 61ef      	ORI  R30,LOW(0x1F)
0034c4   +  	__PUTW1R 16,17
0034c4 2f0e      	MOV  R0x10,R30
0034c5 2f1f      	MOV  R0x11,R31
          	.ENDM
         ;    7185      for(j = 0;j < 4;j++)
0034c6 e030      	LDI  R19,LOW(0)
          _0x785:
0034c7 3034      	CPI  R19,4
0034c8 f4f8      	BRSH _0x786
         ;    7186      {  
         ;    7187         PORTB|= 0x20;    // Start bit   
0034c9 9ac5      	SBI  0x18,5
         ;    7188         Delay110mk();                 
0034ca d063      	RCALL _Delay110mk
         ;    7189         for(ind = 0;ind < 12; ind++){  
0034cb e020      	LDI  R18,LOW(0)
          _0x788:
0034cc 302c      	CPI  R18,12
0034cd f4a0      	BRSH _0x789
         ;    7190             PORTB = PORTB &~0x20;   
0034ce 940e 4e87 	CALL SUBOPT_0xFA
0034d0 bbe8      	OUT  0x18,R30
         ;    7191             Delay110mk();            
0034d1 d05c      	RCALL _Delay110mk
         ;    7192             if((Data_Rely>>ind) & 0x01){   // 1           
0034d2 2fe2      	MOV  R30,R18
0034d3   +  	__GETW2R 16,17
0034d3 2fa0      	MOV  R26,R0x10
0034d4 2fb1      	MOV  R27,R0x11
          	.ENDM
0034d5 27ff      	CLR  R31
0034d6 940e 5044 	CALL __ASRW12
0034d8 70e1      	ANDI R30,LOW(0x1)
0034d9 f019      	BREQ _0x78A
         ;    7193                  Delay110mk();   
0034da d053      	RCALL _Delay110mk
         ;    7194                  PORTB|= 0x20;               
0034db 9ac5      	SBI  0x18,5
         ;    7195             }
         ;    7196             else{            
0034dc c002      	RJMP _0x78B
          _0x78A:
         ;    7197                  PORTB|= 0x20;              // 0 
0034dd 9ac5      	SBI  0x18,5
         ;    7198                  Delay110mk();                                                             
0034de d04f      	RCALL _Delay110mk
         ;    7199             }  
          _0x78B:
         ;    7200             Delay110mk();                                 
0034df d04e      	RCALL _Delay110mk
         ;    7201         }  
0034e0 5f2f      	SUBI R18,-1
0034e1 cfea      	RJMP _0x788
          _0x789:
         ;    7202         PORTB = PORTB &~0x20;   
0034e2 940e 4e87 	CALL SUBOPT_0xFA
0034e4 bbe8      	OUT  0x18,R30
         ;    7203         Delay2_5Ms();       
0034e5 d051      	RCALL _Delay2_5Ms
         ;    7204      }		
0034e6 5f3f      	SUBI R19,-1
0034e7 cfdf      	RJMP _0x785
          _0x786:
         ;    7205 }
0034e8 940e 50a8 	CALL __LOADLOCR4
0034ea 9625      	ADIW R28,5
0034eb 9508      	RET
         ;    7206 void RelayOn(char Relay)
         ;    7207 {           
          _RelayOn:
         ;    7208      char i;       
         ;    7209      for(i = 0; i < 8; i++)
0034ec 930a      	ST   -Y,R16
         ;	Relay -> Y+1
         ;	i -> R16
0034ed e000      	LDI  R16,LOW(0)
          _0x78E:
0034ee 3008      	CPI  R16,8
0034ef f460      	BRSH _0x78F
         ;    7210         if((RelaysOut[i]&0x0F) == Relay)
0034f0 940e 4e8b 	CALL SUBOPT_0xFB
0034f2 f439      	BRNE _0x790
         ;    7211             LongRalay|= (0x01 << i);   
0034f3 940e 4e95 	CALL SUBOPT_0xFC
0034f5 91a0 07ae 	LDS  R26,_LongRalay
0034f7 2bea      	OR   R30,R26
0034f8 93e0 07ae 	STS  _LongRalay,R30
         ;    7212      SendToRelay(LongRalay);        
          _0x790:
0034fa 5f0f      	SUBI R16,-1
0034fb cff2      	RJMP _0x78E
          _0x78F:
0034fc 940e 4e9a 	CALL SUBOPT_0xFD
         ;    7213              
         ;    7214 }      
0034fe c013      	RJMP _0xAB0
         ;    7215 void RelayOff(char Relay)
         ;    7216 {
          _RelayOff:
         ;    7217      char i;     
         ;    7218      for(i = 0; i < 8; i++)
0034ff 930a      	ST   -Y,R16
         ;	Relay -> Y+1
         ;	i -> R16
003500 e000      	LDI  R16,LOW(0)
          _0x793:
003501 3008      	CPI  R16,8
003502 f468      	BRSH _0x794
         ;    7219         if((RelaysOut[i]&0x0F) == Relay)
003503 940e 4e8b 	CALL SUBOPT_0xFB
003505 f441      	BRNE _0x795
         ;    7220             LongRalay&= ~(0x01 << i);   
003506 940e 4e95 	CALL SUBOPT_0xFC
003508 95e0      	COM  R30
003509 91a0 07ae 	LDS  R26,_LongRalay
00350b 23ea      	AND  R30,R26
00350c 93e0 07ae 	STS  _LongRalay,R30
         ;    7221      SendToRelay(LongRalay);  
          _0x795:
00350e 5f0f      	SUBI R16,-1
00350f cff1      	RJMP _0x793
          _0x794:
003510 940e 4e9a 	CALL SUBOPT_0xFD
         ;    7222 }
          _0xAB0:
003512 8108      	LDD  R16,Y+0
003513 9622      	ADIW R28,2
003514 9508      	RET
         ;    7223 void ReadRelayOut(void)
         ;    7224 {
          _ReadRelayOut:
         ;    7225      char i;
         ;    7226      Pointer = ZoneOutPut;
003515 930a      	ST   -Y,R16
         ;	i -> R16
003516 e7e8      	LDI  R30,LOW(632)
003517 e0f2      	LDI  R31,HIGH(632)
003518 940e 4966 	CALL SUBOPT_0x3D
         ;    7227      MaxNum = 16;
         ;    7228      ReadData(1);
00351a 940e 4ba4 	CALL SUBOPT_0x8D
         ;    7229      for(i = 0; i<16; i++)
00351c e000      	LDI  R16,LOW(0)
          _0x798:
00351d 3100      	CPI  R16,16
00351e f468      	BRSH _0x799
         ;    7230          RelaysOut[i] = Data[i];
00351f 2fe0      	MOV  R30,R16
003520 27ff      	CLR  R31
003521 56e2      	SUBI R30,LOW(-_RelaysOut)
003522 4ff8      	SBCI R31,HIGH(-_RelaysOut)
003523 93ff      	PUSH R31
003524 93ef      	PUSH R30
003525 940e 4af2 	CALL SUBOPT_0x71
003527 91af      	POP  R26
003528 91bf      	POP  R27
003529 93ec      	ST   X,R30
00352a 5f0f      	SUBI R16,-1
00352b cff1      	RJMP _0x798
          _0x799:
         ;    7231 } 
00352c 9109      	LD   R16,Y+
00352d 9508      	RET
         ;    7232 void Delay110mk(void)     // delay of 70 Micro
         ;    7233 {
          _Delay110mk:
         ;    7234     char i;
         ;    7235     for(i = 0; i < 56;i++)
00352e 930a      	ST   -Y,R16
         ;	i -> R16
