0005c5 e1f8      	LDI  R31,0x18
0005c6 bdf1      	OUT  WDTCR,R31
0005c7 e1f0      	LDI  R31,0x10
0005c8 bdf1      	OUT  WDTCR,R31
         
         ;CLEAR R2-R15
0005c9 e08e      	LDI  R24,14
0005ca e0a2      	LDI  R26,2
0005cb 27bb      	CLR  R27
          __CLEAR_REG:
0005cc 93ed      	ST   X+,R30
0005cd 958a      	DEC  R24
0005ce f7e9      	BRNE __CLEAR_REG
         
         ;CLEAR SRAM
0005cf e080      	LDI  R24,LOW(0x1000)
0005d0 e190      	LDI  R25,HIGH(0x1000)
0005d1 e0a0      	LDI  R26,LOW(0x100)
0005d2 e0b1      	LDI  R27,HIGH(0x100)
          __CLEAR_SRAM:
0005d3 93ed      	ST   X+,R30
0005d4 9701      	SBIW R24,1
0005d5 f7e9      	BRNE __CLEAR_SRAM
         
         ;GLOBAL VARIABLES INITIALIZATION
0005d6 e7e4      	LDI  R30,LOW(__GLOBAL_INI_TBL*2)
0005d7 e0fb      	LDI  R31,HIGH(__GLOBAL_INI_TBL*2)
          __GLOBAL_INI_NEXT:
0005d8 9005      	LPM  R0,Z+
0005d9 9015      	LPM  R1,Z+
0005da 01bf      	MOVW R22,R30
0005db 01f0      	MOVW R30,R0
0005dc 9730      	SBIW R30,0
0005dd f051      	BREQ __GLOBAL_INI_END
0005de 91a5      	LPM  R26,Z+
0005df 91b5      	LPM  R27,Z+
0005e0 9185      	LPM  R24,Z+
0005e1 9195      	LPM  R25,Z+
          __GLOBAL_INI_LOOP:
0005e2 9005      	LPM  R0,Z+
0005e3 920d      	ST   X+,R0
0005e4 9701      	SBIW R24,1
0005e5 f7e1      	BRNE __GLOBAL_INI_LOOP
0005e6 01fb      	MOVW R30,R22
0005e7 cff0      	RJMP __GLOBAL_INI_NEXT
          __GLOBAL_INI_END:
         
