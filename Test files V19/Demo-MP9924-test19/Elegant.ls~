          	.ENDM
002ce7 93fa      	ST   -Y,R31
002ce8 93ea      	ST   -Y,R30
002ce9 940e 4fdf 	CALL _strcpyf
         ;    6465                                 if(ButtonChime & 0x03)
002ceb 91e0 065d 	LDS  R30,_ButtonChime
002ced 70e3      	ANDI R30,LOW(0x3)
002cee f021      	BREQ _0x691
         ;    6466                                       StrSpike[0] = 'P';   
002cef 81ac      	LDD  R26,Y+4
