         ;   10046 void ReadCountMadCode(void)   // Read counter mad code from eeprom
         ;   10047 {
          _ReadCountMadCode:
         ;   10048     Pointer = Status_SAC;
004499 e1e8      	LDI  R30,LOW(536)
00449a e0f2      	LDI  R31,HIGH(536)
00449b 940e 4966 	CALL SUBOPT_0x3D
         ;   10049     MaxNum = 16;
         ;   10050     ReadData(1);
00449d 940e 4ba4 	CALL SUBOPT_0x8D
         ;   10051     CountMadCode = Data[5];
00449f   +  	__GETB1MN _Data,5
00449f 91e0 05f2 	LDS  R30,_data+0x5
          	.ENDM
0044a1 93e0 0671 	STS  _CountMadCode,R30
         ;   10052 }  
