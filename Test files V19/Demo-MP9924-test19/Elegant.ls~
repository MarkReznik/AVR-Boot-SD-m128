001f87 93ec      	ST   X,R30
         ;    3980      DataMoked[pStopEvent][1] = 0;
001f88 940e 4933 	CALL SUBOPT_0x36
001f8a 940e 4985 	CALL SUBOPT_0x42
         ;    3981      pStopEvent++;
         ;    3982      if(pStopEvent >= MAX_EVENT)
001f8c f018      	BRLO _0x497
         ;    3983         pStopEvent = 0;
