002114 93e0 064f 	STS  _SirenWith,R30
002116 93f0 0650 	STS  _SirenWith+1,R31
         ;    4232      if(CounterSecond >= 250)
002118 91a0 0673 	LDS  R26,_CounterSecond
00211a 3faa      	CPI  R26,LOW(0xFA)
00211b f410      	BRSH PC+3
00211c 940c 2182 	JMP _0x4C1
         ;    4233      {              
         ;    4234           CounterSecond = 0;
