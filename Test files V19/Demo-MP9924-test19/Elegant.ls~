         ;    4495 WriteData(5);      
         ;    4496 strcpyf(Data,"Rivka        ");       
0023c7   +  	__POINTW1FN _1241,665
0023c7 e9e5      	LDI  R30,LOW(2*_1241+0x299)
0023c8 e0f3      	LDI  R31,HIGH(2*_1241+0x299)
          	.ENDM
0023c9 940e 4c3f 	CALL SUBOPT_0xA1
         ;    4497 WriteData(8);    
         ;    4498 strcpyf(Data,"             ");       
0023cb   +  	__POINTW1FN _1241,679
0023cb eae3      	LDI  R30,LOW(2*_1241+0x2a7)
0023cc e0f3      	LDI  R31,HIGH(2*_1241+0x2a7)
          	.ENDM
0023cd 940e 4c4c 	CALL SUBOPT_0xA2
         ;    4499 WriteData(9);    
         ;    4500 strcpyf(Data,"             ");       
0023cf   +  	__POINTW1FN _1241,693
0023cf ebe1      	LDI  R30,LOW(2*_1241+0x2b5)
0023d0 e0f3      	LDI  R31,HIGH(2*_1241+0x2b5)
          	.ENDM
0023d1 940e 4c59 	CALL SUBOPT_0xA3
         ;    4501 WriteData(10);   
         ;    4502 strcpyf(Data,"Room of Alex ");       
0023d3 eeed      	LDI  R30,LOW(_Data)
0023d4 e0f5      	LDI  R31,HIGH(_Data)
0023d5 93fa      	ST   -Y,R31
0023d6 93ea      	ST   -Y,R30
0023d7   +  	__POINTW1FN _1241,707
0023d7 ebef      	LDI  R30,LOW(2*_1241+0x2c3)
0023d8 e0f3      	LDI  R31,HIGH(2*_1241+0x2c3)
          	.ENDM
0023d9 93fa      	ST   -Y,R31
0023da 93ea      	ST   -Y,R30
0023db 940e 4fdf 	CALL _strcpyf
         ;    4503 WriteData(11);    
0023dd e0eb      	LDI  R30,LOW(11)
0023de 93ea      	ST   -Y,R30
0023df d00e      	RCALL _WriteData
         ;    4504 strcpyf(Data,"             ");       
0023e0 eeed      	LDI  R30,LOW(_Data)
0023e1 e0f5      	LDI  R31,HIGH(_Data)
0023e2 93fa      	ST   -Y,R31
0023e3 93ea      	ST   -Y,R30
0023e4   +  	__POINTW1FN _1241,721
0023e4 eced      	LDI  R30,LOW(2*_1241+0x2d1)
0023e5 e0f3      	LDI  R31,HIGH(2*_1241+0x2d1)
          	.ENDM
0023e6 93fa      	ST   -Y,R31
0023e7 93ea      	ST   -Y,R30
0023e8 940e 4fdf 	CALL _strcpyf
         ;    4505 WriteData(12);    
0023ea e0ec      	LDI  R30,LOW(12)
0023eb 93ea      	ST   -Y,R30
0023ec d001      	RCALL _WriteData
         ;    4506 }
0023ed 9508      	RET
