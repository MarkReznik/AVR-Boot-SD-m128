         ;    4522 sfrb PINF=0;
         ;    4523 sfrb PINE=1;
         ;    4524 sfrb DDRE=2;
         ;    4525 sfrb PORTE=3;
         ;    4526 sfrb ADCL=4;
         ;    4527 sfrb ADCH=5;
         ;    4528 sfrw ADCW=4;      // 16 bit access
         ;    4529 sfrb ADCSRA=6;
         ;    4530 sfrb ADMUX=7;
         ;    4531 sfrb ACSR=8;
         ;    4532 sfrb UBRR0L=9;
         ;    4533 sfrb UCSR0B=0xa;
         ;    4534 sfrb UCSR0A=0xb;
         ;    4535 sfrb UDR0=0xc;
