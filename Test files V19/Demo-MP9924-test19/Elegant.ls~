         ;     169 sfrb ICR1H=0x27;
         ;     170 sfrw ICR1=0x26;   // 16 bit access
         ;     171 sfrb OCR1BL=0x28;
         ;     172 sfrb OCR1BH=0x29;
         ;     173 sfrw OCR1B=0x28;  // 16 bit access
         ;     174 sfrb OCR1AL=0x2a;
         ;     175 sfrb OCR1AH=0x2b;
         ;     176 sfrw OCR1A=0x2a;  // 16 bit access
         ;     177 sfrb TCNT1L=0x2c;
         ;     178 sfrb TCNT1H=0x2d;
         ;     179 sfrw TCNT1=0x2c;  // 16 bit access
         ;     180 sfrb TCCR1B=0x2e;
         ;     181 sfrb TCCR1A=0x2f;
         ;     182 sfrb ASSR=0x30;
