         ;    9744 } 
00445a 9508      	RET
         ;    9745 void Read_Date(void)
         ;    9746 {        
          _Read_Date:
         ;    9747      Pointer = TimeDate;      
00445b eee0      	LDI  R30,LOW(480)
00445c e0f1      	LDI  R31,HIGH(480)
00445d 93e0 0691 	STS  _Pointer,R30
00445f 93f0 0692 	STS  _Pointer+1,R31
         ;    9748      MaxNum = 4;   
004461 e0e4      	LDI  R30,LOW(4)
004462 940e 4b23 	CALL SUBOPT_0x79
         ;    9749      ReadData(1);  
         ;    9750      Day = Data[0];
004464 93e0 07e2 	STS  _Day,R30
         ;    9751      Month = Data[1];
004466   +  	__GETB1MN _Data,1
004466 91e0 05ee 	LDS  R30,_data+0x1
          	.ENDM
004468 93e0 07e1 	STS  _Month,R30
         ;    9752      Year = Data[2];   
00446a   +  	__GETB1MN _Data,2
00446a 91e0 05ef 	LDS  R30,_data+0x2
          	.ENDM
00446c 93e0 07e0 	STS  _Year,R30
         ;    9753      DayWeek = Data[3];       
