         ;   10473               if(Data[NextCode] == '*')
004660 940e 4b52 	CALL SUBOPT_0x7F
004662 32ea      	CPI  R30,LOW(0x2A)
004663 f449      	BRNE _0x9F1
         ;   10474                       Data[NextCode] = 0x31 + NextCode;
