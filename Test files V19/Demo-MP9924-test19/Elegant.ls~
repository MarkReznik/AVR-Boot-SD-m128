004669 5cef      	SUBI R30,-LOW(49)
00466a 91af      	POP  R26
00466b 91bf      	POP  R27
00466c c003      	RJMP _0xB05
         ;   10475               else
          _0x9F1:
         ;   10476                       Data[NextCode] = '*'; 
00466d 940e 4b61 	CALL SUBOPT_0x82
00466f e2ea      	LDI  R30,LOW(42)
          _0xB05:
004670 93ec      	ST   X,R30
         ;   10477               NextCode++;                  
004671 940e 4b58 	CALL SUBOPT_0x80
         ;   10478        }else
004673 c084      	RJMP _0x9F3
          _0x9EE:
         ;   10479        if(NextCode == 8){
004674 81a8      	LD   R26,Y
